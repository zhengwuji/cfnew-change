        }
        .login-button {
            width: 100%;
            padding: 14px;
            background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
            color: white;
            border: none;
            border-radius: 10px;
            font-size: 16px;
            font-weight: 600;
            cursor: pointer;
            transition: transform 0.2s, box-shadow 0.2s;
        }
        .login-button:hover {
            transform: translateY(-2px);
            box-shadow: 0 10px 20px rgba(102, 126, 234, 0.4);
        }
        .login-button:active {
            transform: translateY(0);
        }
        .error-message {
            background: #fee;
            border: 1px solid #fcc;
            color: #c33;
            padding: 12px;
            border-radius: 8px;
            margin-bottom: 20px;
            display: none;
            font-size: 14px;
        }
        .error-message.show {
            display: block;
        }
        .footer-text {
            text-align: center;
            margin-top: 20px;
            color: #999;
            font-size: 12px;
        }
    </style>
</head>
<body>
    <div class="login-container">
        <div class="login-header">
            <h1>üîê Á≥ªÁªüÁôªÂΩï</h1>
            <p>ËØ∑ËæìÂÖ•ÊÇ®ÁöÑÁôªÂΩïÂá≠ËØÅ</p>
        </div>
        <div class="error-message" id="errorMessage"></div>
        <form id="loginForm">
            <div class="form-group">
                <label for="username">Áî®Êà∑Âêç</label>
                <input type="text" id="username" name="username" placeholder="ËØ∑ËæìÂÖ•Áî®Êà∑Âêç" required autofocus>
            </div>
            <div class="form-group">
                <label for="password">ÂØÜÁ†Å</label>
                <input type="password" id="password" name="password" placeholder="ËØ∑ËæìÂÖ•ÂØÜÁ†Å" required>
            </div>
            <button type="submit" class="login-button">Áôª ÂΩï</button>
        </form>
        <div class="footer-text">
            Powered by Cloudflare Workers
        </div>
    </div>
    <script>
        document.getElementById('loginForm').addEventListener('submit', async (e) => {
            e.preventDefault();
            const errorMsg = document.getElementById('errorMessage');
            const username = document.getElementById('username').value;
            const password = document.getElementById('password').value;
            
            try {
                const response = await fetch('/login', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify({ username, password })
                });
                
                const data = await response.json();
                
                if (data.success) {
                    // ÁôªÂΩïÊàêÂäüÔºåË∑≥ËΩ¨Âà∞ËÆ¢ÈòÖÈ°µÈù¢
                    window.location.href = data.redirect || ('/' + data.uuid);
                } else {
                    errorMsg.textContent = data.message || 'ÁôªÂΩïÂ§±Ë¥•ÔºåËØ∑Ê£ÄÊü•Áî®Êà∑ÂêçÂíåÂØÜÁ†Å';
                    errorMsg.classList.add('show');
                    setTimeout(() => {
                        errorMsg.classList.remove('show');
                    }, 3000);
                }
            } catch (error) {
                errorMsg.textContent = 'ÁôªÂΩïËØ∑Ê±ÇÂ§±Ë¥•ÔºåËØ∑Á®çÂêéÈáçËØï';
                errorMsg.classList.add('show');
            }
        });
    </script>
</body>
</html>`;

    return new Response(loginHtml, {
        status: 200,
        headers: { 'Content-Type': 'text/html; charset=utf-8' }
    });
}

// ÁôªÂΩïËÆ§ËØÅÂ§ÑÁêÜÂáΩÊï∞
async function handleLoginAuth(request, env) {
    if (request.method !== 'POST') {
        return new Response(JSON.stringify({ success: false, message: 'Êó†ÊïàÁöÑËØ∑Ê±ÇÊñπÊ≥ï' }), {
            status: 405,
            headers: { 'Content-Type': 'application/json' }
        });
    }

    try {
        const body = await request.json();
        const { username, password } = body;

        // È™åËØÅÁî®Êà∑ÂêçÂíåÂØÜÁ†Å
        if (username === loginUsername && password === loginPassword) {
            // ÁîüÊàêËÆ§ËØÅ cookie
            const authCookie = await generateAuthCookie(username);

            return new Response(JSON.stringify({
                success: true,
                message: 'ÁôªÂΩïÊàêÂäü',
                uuid: at,
                redirect: '/' + at
            }), {
                status: 200,
                headers: {
                    'Content-Type': 'application/json',
                    'Set-Cookie': `auth_token=${authCookie}; Path=/; Max-Age=86400; HttpOnly; SameSite=Strict`
                }
            });
        } else {
            return new Response(JSON.stringify({
                success: false,
                message: 'Áî®Êà∑ÂêçÊàñÂØÜÁ†ÅÈîôËØØ'
            }), {
                status: 401,
                headers: { 'Content-Type': 'application/json' }
            });
        }
    } catch (error) {
        return new Response(JSON.stringify({
            success: false,
            message: 'ËØ∑Ê±ÇÊï∞ÊçÆÊ†ºÂºèÈîôËØØ'
        }), {
            status: 400,
            headers: { 'Content-Type': 'application/json' }
        });
    }
}
