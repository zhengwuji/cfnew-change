// ÈîôËØØÊó•ÂøóÁ≥ªÁªü
let errorLogs = [];
const MAX_ERROR_LOGS = 1000; // ÊúÄÂ§ö‰øùÂ≠ò1000Êù°ÈîôËØØÊó•Âøó

/**
 * ËÆ∞ÂΩïÈîôËØØÊó•Âøó
 * @param {Error|string} error - ÈîôËØØÂØπË±°ÊàñÈîôËØØÊ∂àÊÅØ
 * @param {string} context - ÈîôËØØÂèëÁîüÁöÑ‰∏ä‰∏ãÊñáÔºàÂáΩÊï∞Âêç„ÄÅÊìç‰ΩúÁ≠âÔºâ
 * @param {Object} extra - È¢ùÂ§ñÁöÑÈîôËØØ‰ø°ÊÅØÔºàËØ∑Ê±ÇURL„ÄÅÁî®Êà∑Á≠âÔºâ
 */
function logError(error, context = 'Unknown', extra = {}) {
    try {
        const errorEntry = {
            timestamp: new Date().toISOString(),
            context: context,
            message: error instanceof Error ? error.message : String(error),
            stack: error instanceof Error ? error.stack : undefined,
            type: error instanceof Error ? error.constructor.name : 'String',
            url: extra.url || '',
            method: extra.method || '',
            user: extra.user || '',
            workerRegion: extra.workerRegion || currentWorkerRegion || '',
            statusCode: extra.statusCode || '',
            extra: JSON.stringify(extra)
        };
        
        errorLogs.unshift(errorEntry); // Ê∑ªÂä†Âà∞Êï∞ÁªÑÂºÄÂ§¥
        
        // ÈôêÂà∂Êó•ÂøóÊï∞Èáè
        if (errorLogs.length > MAX_ERROR_LOGS) {
            errorLogs = errorLogs.slice(0, MAX_ERROR_LOGS);
        }
        
        // ÂêåÊó∂ËæìÂá∫Âà∞ÊéßÂà∂Âè∞ÔºàCloudflare Workers Êó•ÂøóÔºâ
        console.error(`[ERROR-LOG] [${errorEntry.timestamp}] [${context}] ${errorEntry.message}`);
        if (errorEntry.stack) {
            console.error(`[ERROR-STACK] ${errorEntry.stack}`);
        }
        if (errorEntry.statusCode) {
            console.error(`[ERROR-STATUS] HTTP ${errorEntry.statusCode}`);
        }
    } catch (logErr) {
        // Â¶ÇÊûúËÆ∞ÂΩïÊó•ÂøóÊú¨Ë∫´Âá∫ÈîôÔºåÂè™ËæìÂá∫Âà∞ÊéßÂà∂Âè∞
        console.error('[ERROR-LOG] Failed to log error:', logErr);
    }
}

/**
 * ËÆ∞ÂΩï HTTP ÈîôËØØÂìçÂ∫î
 * @param {number} statusCode - HTTP Áä∂ÊÄÅÁ†Å
 * @param {string} message - ÈîôËØØÊ∂àÊÅØ
 * @param {string} context - ÈîôËØØÂèëÁîüÁöÑ‰∏ä‰∏ãÊñá
 * @param {Object} extra - È¢ùÂ§ñÁöÑÈîôËØØ‰ø°ÊÅØ
 */
function logHTTPError(statusCode, message, context = 'HTTPError', extra = {}) {
    // Âè™ËÆ∞ÂΩï 4xx Âíå 5xx ÈîôËØØ
    if (statusCode >= 400) {
        // ËøáÊª§Êéâ‰∏çÈáçË¶ÅÁöÑËØ∑Ê±ÇÔºàÊµèËßàÂô®Ëá™Âä®ËØ∑Ê±ÇÔºâ
        const ignoredPaths = [
            '/favicon.ico',
            '/robots.txt',
            '/apple-touch-icon.png',
            '/apple-touch-icon-precomposed.png',
            '/favicon.png',
            '/favicon.jpg',
            '/.well-known/',
            '/sitemap.xml'
        ];
        
        const url = extra.url || '';
        const shouldIgnore = ignoredPaths.some(path => url.includes(path));
        
        // ÂØπ‰∫é 404 ÈîôËØØÔºåÂ¶ÇÊûúÊòØË¢´ÂøΩÁï•ÁöÑË∑ØÂæÑÔºåÂàô‰∏çËÆ∞ÂΩï
        // ÂØπ‰∫éÂÖ∂‰ªñÈîôËØØÔºà403, 500Á≠âÔºâÔºå‰ªçÁÑ∂ËÆ∞ÂΩï
        if (statusCode === 404 && shouldIgnore) {
            return; // ‰∏çËÆ∞ÂΩï favicon.ico Á≠â 404 ÈîôËØØ
        }
        
        logError(new Error(`HTTP ${statusCode}: ${message}`), context, {
            ...extra,
            statusCode: statusCode
        });
    }
}

/**
 * ÂàõÂª∫ÂìçÂ∫îÂπ∂Ëá™Âä®ËÆ∞ÂΩïÈîôËØØ
 * @param {string|Object} body - ÂìçÂ∫î‰Ωì
 * @param {Object} options - ÂìçÂ∫îÈÄâÈ°πÔºàstatus, headersÁ≠âÔºâ
 * @param {string} context - ‰∏ä‰∏ãÊñá
 * @param {Object} extra - È¢ùÂ§ñ‰ø°ÊÅØ
 * @returns {Response}
 */
function createResponse(body, options = {}, context = 'Response', extra = {}) {
    const status = options.status || 200;
    
    // Â¶ÇÊûúÊòØÈîôËØØÂìçÂ∫îÔºåËÆ∞ÂΩïÊó•Âøó
    if (status >= 400) {
        let message = '';
        if (typeof body === 'string') {
            message = body;
        } else if (body && typeof body === 'object') {
            try {
                message = JSON.stringify(body);
            } catch (e) {
                message = 'Error response';
            }
        }
        logHTTPError(status, message, context, {
            ...extra,
            url: extra.url || '',
            method: extra.method || '',
            user: extra.user || '',
            workerRegion: extra.workerRegion || currentWorkerRegion || ''
        });
    }
    
    // ÂàõÂª∫ÂìçÂ∫î
    if (typeof body === 'object' && !(body instanceof Response)) {
        return new Response(JSON.stringify(body), {
            ...options,
            headers: {
                'Content-Type': 'application/json; charset=utf-8',
                ...options.headers
            }
        });
    }
    
    return new Response(body, options);
}

/**
 * Ëé∑ÂèñÈîôËØØÊó•ÂøóÔºàÊîØÊåÅËøáÊª§ÂíåÂàÜÈ°µÔºâ
 * @param {Object} options - Êü•ËØ¢ÈÄâÈ°π
 * @returns {Array} ÈîôËØØÊó•ÂøóÊï∞ÁªÑ
 */
function getErrorLogs(options = {}) {
    let logs = [...errorLogs];
    
    // ÊåâÊó∂Èó¥ËøáÊª§
    if (options.since) {
        const sinceTime = new Date(options.since).getTime();
        logs = logs.filter(log => new Date(log.timestamp).getTime() >= sinceTime);
    }
    
    // Êåâ‰∏ä‰∏ãÊñáËøáÊª§
    if (options.context) {
        logs = logs.filter(log => log.context.includes(options.context));
    }
    
    // ÊåâÊ∂àÊÅØËøáÊª§
    if (options.message) {
        logs = logs.filter(log => log.message.includes(options.message));
    }
    
    // ÂàÜÈ°µ
    const limit = options.limit || 100;
    const offset = options.offset || 0;
    logs = logs.slice(offset, offset + limit);
    
    return logs;
}

/**
 * Ê∏ÖÁ©∫ÈîôËØØÊó•Âøó
 */
function clearErrorLogs() {
    errorLogs = [];
}

const ADDRESS_TYPE_IPV4 = 1;
const ADDRESS_TYPE_URL = 2;
const ADDRESS_TYPE_IPV6 = 3;

function isValidFormat(str) {
    const userRegex = /^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$/i;
    return userRegex.test(str);
}

function isValidIP(ip) {
    const ipv4Regex = /^(?:(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.){3}(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)$/;
    if (ipv4Regex.test(ip)) return true;

    const ipv6Regex = /^(?:[0-9a-fA-F]{1,4}:){7}[0-9a-fA-F]{1,4}$/;
    if (ipv6Regex.test(ip)) return true;

    const ipv6ShortRegex = /^::1$|^::$|^(?:[0-9a-fA-F]{1,4}:)*::(?:[0-9a-fA-F]{1,4}:)*[0-9a-fA-F]{1,4}$/;
    if (ipv6ShortRegex.test(ip)) return true;

    return false;
}

async function initKVStore(env) {

    if (env.C) {
        try {
            kvStore = env.C;
            await loadKVConfig();
        } catch (error) {
            kvStore = null;
        }
    }
}

async function loadKVConfig() {

    if (!kvStore) {
        return;
    }

    try {
        const configData = await kvStore.get('c');

        if (configData) {
            kvConfig = JSON.parse(configData);
        }
    } catch (error) {
        kvConfig = {};
    }
}

async function saveKVConfig() {
    if (!kvStore) {
        return;
    }

    try {
        const configString = JSON.stringify(kvConfig);
        await kvStore.put('c', configString);
    } catch (error) {
        throw error;
    }
}

function getConfigValue(key, defaultValue = '') {

    if (kvConfig[key] !== undefined) {
        return kvConfig[key];
    }
    return defaultValue;
}

async function setConfigValue(key, value) {
    kvConfig[key] = value;
    await saveKVConfig();
}

async function detectWorkerRegion(request) {
    try {
        const cfCountry = request.cf?.country;

        if (cfCountry) {
            const countryToRegion = {
                'US': 'US', 'SG': 'SG', 'JP': 'JP', 'KR': 'KR',
                'DE': 'DE', 'SE': 'SE', 'NL': 'NL', 'FI': 'FI', 'GB': 'GB',
                'CN': 'SG', 'TW': 'JP', 'AU': 'SG', 'CA': 'US',
                'FR': 'DE', 'IT': 'DE', 'ES': 'DE', 'CH': 'DE',
                'AT': 'DE', 'BE': 'NL', 'DK': 'SE', 'NO': 'SE', 'IE': 'GB'
            };

            if (countryToRegion[cfCountry]) {
                return countryToRegion[cfCountry];
            }
        }

        return 'SG';

    } catch (error) {
        return 'SG';
    }
}

async function checkIPAvailability(domain, port = 443, timeout = 2000) {
    try {
        const controller = new AbortController();
        const timeoutId = setTimeout(() => controller.abort(), timeout);

        const response = await fetch(`https://${domain}`, {
            method: 'HEAD',
            signal: controller.signal,
            headers: {
                'User-Agent': 'Mozilla/5.0 (compatible; CF-IP-Checker/1.0)'
            }
        });

        clearTimeout(timeoutId);
        return response.status < 500;
    } catch (error) {
        return true;
    }
}

async function getBestBackupIP(workerRegion = '') {

    if (backupIPs.length === 0) {
        return null;
    }

    const availableIPs = backupIPs.map(ip => ({ ...ip, available: true }));

    if (enableRegionMatching && workerRegion) {
        const sortedIPs = getSmartRegionSelection(workerRegion, availableIPs);
        if (sortedIPs.length > 0) {
            const selectedIP = sortedIPs[0];
            return selectedIP;
        }
    }

    const selectedIP = availableIPs[0];
    return selectedIP;
}

function getNearbyRegions(region) {
    const nearbyMap = {
        'US': ['SG', 'JP', 'KR'],
        'SG': ['JP', 'KR', 'US'],
        'JP': ['SG', 'KR', 'US'],
        'KR': ['JP', 'SG', 'US'],
        'DE': ['NL', 'GB', 'SE', 'FI'],
        'SE': ['DE', 'NL', 'FI', 'GB'],
        'NL': ['DE', 'GB', 'SE', 'FI'],
        'FI': ['SE', 'DE', 'NL', 'GB'],
        'GB': ['DE', 'NL', 'SE', 'FI']
    };

    return nearbyMap[region] || [];
}

function getAllRegionsByPriority(region) {
    const nearbyRegions = getNearbyRegions(region);
    const allRegions = ['US', 'SG', 'JP', 'KR', 'DE', 'SE', 'NL', 'FI', 'GB'];

    return [region, ...nearbyRegions, ...allRegions.filter(r => r !== region && !nearbyRegions.includes(r))];
}

function getSmartRegionSelection(workerRegion, availableIPs) {

    if (!enableRegionMatching || !workerRegion) {
        return availableIPs;
    }

    const priorityRegions = getAllRegionsByPriority(workerRegion);

    const sortedIPs = [];

    for (const region of priorityRegions) {
        const regionIPs = availableIPs.filter(ip => ip.regionCode === region);
        sortedIPs.push(...regionIPs);
    }

    return sortedIPs;
}

function parseAddressAndPort(input) {
    if (input.includes('[') && input.includes(']')) {
        const match = input.match(/^\[([^\]]+)\](?::(\d+))?$/);
        if (match) {
            return {
                address: match[1],
                port: match[2] ? parseInt(match[2], 10) : null
            };
        }
    }

    const lastColonIndex = input.lastIndexOf(':');
    if (lastColonIndex > 0) {
        const address = input.substring(0, lastColonIndex);
        const portStr = input.substring(lastColonIndex + 1);
        const port = parseInt(portStr, 10);

        if (!isNaN(port) && port > 0 && port <= 65535) {
            return { address, port };
        }
    }

    return { address: input, port: null };
}

export default {
    async fetch(request, env, ctx) {
        try {

            await initKVStore(env);

            // ‰øùÂ≠òÂéüÂßãÁéØÂ¢ÉÂèòÈáèuÁöÑÂÄºÔºàÊîØÊåÅÂ§ßÂ∞èÂÜôÔºâÔºåÁî®‰∫éËÆ¢ÈòÖË∑ØÂæÑÈ™åËØÅ
            envUKey = (env.u || env.U || defaultUUID);

            // Ê£ÄÊü•ÊòØÂê¶ËÆæÁΩÆ‰∫ÜËá™ÂÆö‰πâUUIDÔºàÁî®‰∫éVLESSÂçèËÆÆÔºâ
            const customUUID = getConfigValue('uuid', env.uuid || env.UUID);
            if (customUUID && /^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$/i.test(customUUID)) {
                // Â¶ÇÊûúËÆæÁΩÆ‰∫ÜÊúâÊïàÁöÑËá™ÂÆö‰πâUUIDÔºå‰ΩøÁî®ÂÆÉ
                at = customUUID.toLowerCase();
                vlessUUID = customUUID.toLowerCase();
            } else {
                // Âê¶Âàô‰ΩøÁî®ÈªòËÆ§UUID
                at = defaultUUID.toLowerCase();
                vlessUUID = defaultUUID.toLowerCase();
            }

            const subPath = (env.d || env.D || envUKey).toLowerCase();

            const ci = getConfigValue('p', env.p || env.P);
            let useCustomIP = false;

            const manualRegion = getConfigValue('wk', env.wk || env.WK);
            if (manualRegion && manualRegion.trim()) {
                manualWorkerRegion = manualRegion.trim().toUpperCase();
                currentWorkerRegion = manualWorkerRegion;
            } else if (ci && ci.trim()) {
                useCustomIP = true;
                currentWorkerRegion = 'CUSTOM';
            } else {
                currentWorkerRegion = await detectWorkerRegion(request);
            }

            const regionMatchingControl = env.rm || env.RM;
            if (regionMatchingControl && regionMatchingControl.toLowerCase() === 'no') {
                enableRegionMatching = false;
            }

            const envFallback = getConfigValue('p', env.p || env.P);
            if (envFallback) {
                const fallbackValue = envFallback.toLowerCase();
                if (fallbackValue.includes(']:')) {
                    const lastColonIndex = fallbackValue.lastIndexOf(':');
                    fallbackPort = fallbackValue.slice(lastColonIndex + 1);
                    fallbackAddress = fallbackValue.slice(0, lastColonIndex);
                } else if (!fallbackValue.includes(']:') && !fallbackValue.includes(']')) {
                    [fallbackAddress, fallbackPort = '443'] = fallbackValue.split(':');
                } else {
                    fallbackAddress = fallbackValue;
                    fallbackPort = '443';
                }
            }

            socks5Config = getConfigValue('s', env.s || env.S) || socks5Config;
            if (socks5Config) {
                try {
                    parsedSocks5Config = parseSocksConfig(socks5Config);
                    isSocksEnabled = true;
                } catch (err) {
                    isSocksEnabled = false;
                }
            }

            const customPreferred = getConfigValue('yx', env.yx || env.YX);
            if (customPreferred) {
                try {
                    const preferredList = customPreferred.split(',').map(item => item.trim()).filter(item => item);
                    customPreferredIPs = [];
                    customPreferredDomains = [];

                    preferredList.forEach(item => {

                        let nodeName = '';
                        let addressPart = item;

                        if (item.includes('#')) {
                            const parts = item.split('#');
                            addressPart = parts[0].trim();
                            nodeName = parts[1].trim();
                        }

                        const { address, port } = parseAddressAndPort(addressPart);

                        if (!nodeName) {
                            nodeName = 'Ëá™ÂÆö‰πâ‰ºòÈÄâ-' + address + (port ? ':' + port : '');
                        }

                        if (isValidIP(address)) {
                            customPreferredIPs.push({
                                ip: address,
                                port: port,
                                isp: nodeName
                            });
                        } else {
                            customPreferredDomains.push({
                                domain: address,
                                port: port,
                                name: nodeName
                            });
                        }
                    });
                } catch (err) {
                    customPreferredIPs = [];
                    customPreferredDomains = [];
                }
            }

            const downgradeControl = getConfigValue('qj', env.qj || env.QJ);
            if (downgradeControl && downgradeControl.toLowerCase() === 'no') {
                enableSocksDowngrade = true;
            }

            const dkbyControl = getConfigValue('dkby', env.dkby || env.DKBY);
            if (dkbyControl && dkbyControl.toLowerCase() === 'yes') {
                disableNonTLS = true;
            }

            const yxbyControl = env.yxby || env.YXBY;
            if (yxbyControl && yxbyControl.toLowerCase() === 'yes') {
                disablePreferred = true;
            }

            // WebSocket (VLESS) ÂçèËÆÆÊéßÂà∂ - ÈªòËÆ§ÂêØÁî®ÔºåÂè™ÊúâÊòæÂºèËÆæ‰∏∫ 'no' Êó∂ÊâçÁ¶ÅÁî®
            const vlessControl = getConfigValue('ev', env.ev);
            if (vlessControl === 'no' || vlessControl === false || vlessControl === 'false') {
                ev = false;
            } else {
                ev = true;  // ÈªòËÆ§ÂêØÁî® WebSocket
            }

            // Trojan ÂçèËÆÆÊéßÂà∂ - ÈªòËÆ§Á¶ÅÁî®ÔºåÂè™ÊúâÊòæÂºèËÆæ‰∏∫ 'yes' Êó∂ÊâçÂêØÁî®
            const tjControl = getConfigValue('et', env.et);
            if (tjControl === 'yes' || tjControl === true || tjControl === 'true') {
                et = true;
            } else {
                et = false;  // ÈªòËÆ§Á¶ÅÁî® Trojan
            }

            tp = getConfigValue('tp', env.tp) || '';

            // XHTTP ÂçèËÆÆÊéßÂà∂ - ÈªòËÆ§Á¶ÅÁî®ÔºåÂè™ÊúâÊòæÂºèËÆæ‰∏∫ 'yes' Êó∂ÊâçÂêØÁî®
            const xhttpControl = getConfigValue('ex', env.ex);
            if (xhttpControl === 'yes' || xhttpControl === true || xhttpControl === 'true') {
                ex = true;
            } else {
                ex = false;  // ÈªòËÆ§Á¶ÅÁî® XHTTPÔºå‰ºòÂÖà‰ΩøÁî® WebSocket
            }

            scu = getConfigValue('scu', env.scu) || 'https://url.v1.mk/sub';

            const preferredDomainsControl = getConfigValue('epd', env.epd || 'no');
            if (preferredDomainsControl !== undefined && preferredDomainsControl !== '') {
                epd = preferredDomainsControl !== 'no' && preferredDomainsControl !== false && preferredDomainsControl !== 'false';
            }

            const preferredIPsControl = getConfigValue('epi', env.epi);
            if (preferredIPsControl !== undefined && preferredIPsControl !== '') {
                epi = preferredIPsControl !== 'no' && preferredIPsControl !== false && preferredIPsControl !== 'false';
            }

            const githubIPsControl = getConfigValue('egi', env.egi);
            if (githubIPsControl !== undefined && githubIPsControl !== '') {
                egi = githubIPsControl !== 'no' && githubIPsControl !== false && githubIPsControl !== 'false';
            }

            if (!ev && !et && !ex) {
                ev = true;
            }

            piu = getConfigValue('yxURL', env.yxURL || env.YXURL) || 'https://raw.githubusercontent.com/qwer-search/bestip/refs/heads/main/kejilandbestip.txt';

            cp = getConfigValue('d', env.d || env.D) || '';

            const defaultURL = 'https://raw.githubusercontent.com/qwer-search/bestip/refs/heads/main/kejilandbestip.txt';
            if (piu !== defaultURL) {
                directDomains.length = 0;
                customPreferredIPs = [];
                customPreferredDomains = [];
            }

            const url = new URL(request.url);
            
            // ÊèêÂâçÂ§ÑÁêÜÊµèËßàÂô®Ëá™Âä®ËØ∑Ê±ÇÔºåÈÅøÂÖçËÆ∞ÂΩï‰∏∫ÈîôËØØ
            const ignoredPaths = ['/favicon.ico', '/robots.txt', '/apple-touch-icon.png', '/favicon.png', '/favicon.jpg'];
            if (ignoredPaths.includes(url.pathname)) {
                return new Response('', { status: 204 }); // ËøîÂõû 204 No ContentÔºå‰∏çËÆ∞ÂΩïÊó•Âøó
            }

            if (url.pathname.includes('/api/config')) {
                const pathParts = url.pathname.split('/').filter(p => p);

                const apiIndex = pathParts.indexOf('api');
                if (apiIndex > 0) {
                    const pathSegments = pathParts.slice(0, apiIndex);
                    const pathIdentifier = pathSegments.join('/');

                    let isValid = false;
                    if (cp && cp.trim()) {

                        const cleanCustomPath = cp.trim().startsWith('/') ? cp.trim().substring(1) : cp.trim();
                        isValid = (pathIdentifier === cleanCustomPath);
                    } else {
                        // È™åËØÅÂøÖÈ°ª‰∏éenvUKeyÂÆåÂÖ®ÂåπÈÖçÔºàÊîØÊåÅÂ§ßÂ∞èÂÜôÔºâ
                        isValid = (pathIdentifier === envUKey);
                    }

                    if (isValid) {
                        return await handleConfigAPI(request);
                    } else {
                        return new Response(JSON.stringify({ error: 'Ë∑ØÂæÑÈ™åËØÅÂ§±Ë¥•ÔºåÂøÖÈ°ª‰ΩøÁî®Ê≠£Á°ÆÁöÑÂØÜÈí•u' }), {
                            status: 403,
                            headers: { 'Content-Type': 'application/json; charset=utf-8' }
                        });
                    }
                }
            }



            if (url.pathname.includes('/api/preferred-ips')) {
            }

            if (url.pathname.includes('/api/preferred-ips')) {
                const pathParts = url.pathname.split('/').filter(p => p);

                const apiIndex = pathParts.indexOf('api');
                if (apiIndex > 0) {
                    const pathSegments = pathParts.slice(0, apiIndex);
                    const pathIdentifier = pathSegments.join('/');

                    let isValid = false;
                    if (cp && cp.trim()) {

                        const cleanCustomPath = cp.trim().startsWith('/') ? cp.trim().substring(1) : cp.trim();
                        isValid = (pathIdentifier === cleanCustomPath);
                    } else {
                        // È™åËØÅÂøÖÈ°ª‰∏éenvUKeyÂÆåÂÖ®ÂåπÈÖçÔºàÊîØÊåÅÂ§ßÂ∞èÂÜôÔºâ
                        isValid = (pathIdentifier === envUKey);
                    }

                    if (isValid) {
                        return await handlePreferredIPsAPI(request);
                    } else {
                        return new Response(JSON.stringify({ error: 'Ë∑ØÂæÑÈ™åËØÅÂ§±Ë¥•ÔºåÂøÖÈ°ª‰ΩøÁî®Ê≠£Á°ÆÁöÑÂØÜÈí•u' }), {
                            status: 403,
                            headers: { 'Content-Type': 'application/json; charset=utf-8' }
                        });
                    }
                }

                return new Response(JSON.stringify({ error: 'Êó†ÊïàÁöÑAPIË∑ØÂæÑ' }), {
                    status: 404,
                    headers: { 'Content-Type': 'application/json' }
                });
            }

            // ÈîôËØØÊó•Âøó APIÔºö/{UUIDÊàñËá™ÂÆö‰πâË∑ØÂæÑ}/api/error-logs
            if (url.pathname.includes('/api/error-logs')) {
                const pathParts = url.pathname.split('/').filter(p => p);

                const apiIndex = pathParts.indexOf('api');
                if (apiIndex > 0) {
                    const pathSegments = pathParts.slice(0, apiIndex);
                    const pathIdentifier = pathSegments.join('/');

                    let isValid = false;
                    if (cp && cp.trim()) {

                        const cleanCustomPath = cp.trim().startsWith('/') ? cp.trim().substring(1) : cp.trim();
                        isValid = (pathIdentifier === cleanCustomPath);
                    } else {
                        // È™åËØÅÂøÖÈ°ª‰∏éenvUKeyÂÆåÂÖ®ÂåπÈÖçÔºàÊîØÊåÅÂ§ßÂ∞èÂÜôÔºâ
                        isValid = (pathIdentifier === envUKey);
                    }

                    if (!isValid) {
                        const resp = {
                            error: 'ËÆøÈóÆË¢´ÊãíÁªù',
                            message: 'Ë∑ØÂæÑÈ™åËØÅÂ§±Ë¥•ÔºåÂøÖÈ°ª‰ΩøÁî®Ê≠£Á°ÆÁöÑÂØÜÈí•u'
                        };
                        logHTTPError(403, JSON.stringify(resp), 'ErrorLogsAPI', {
                            url: url.pathname,
                            method: request.method,
                            user: envUKey || 'unknown',
                            workerRegion: currentWorkerRegion || 'unknown'
                        });
                        return new Response(JSON.stringify(resp), {
                            status: 403,
                            headers: { 'Content-Type': 'application/json; charset=utf-8' }
                        });
                    }

                    // ‰ªÖÊîØÊåÅ GET Ëé∑ÂèñÊó•Âøó„ÄÅPOST Ê∏ÖÁ©∫Êó•Âøó
                    if (request.method === 'GET') {
                        const searchParams = url.searchParams;
                        const limit = parseInt(searchParams.get('limit') || '100', 10);
                        const offset = parseInt(searchParams.get('offset') || '0', 10);
                        const context = searchParams.get('context') || '';
                        const message = searchParams.get('message') || '';
                        const since = searchParams.get('since') || '';

                        const options = {
                            limit: isNaN(limit) ? 100 : Math.min(Math.max(limit, 1), 500),
                            offset: isNaN(offset) ? 0 : Math.max(offset, 0)
                        };

                        if (context) options.context = context;
                        if (message) options.message = message;
                        if (since) options.since = since;

                        const logs = getErrorLogs(options);
                        return new Response(JSON.stringify({
                            success: true,
                            total: logs.length,
                            logs
                        }), {
                            status: 200,
                            headers: { 'Content-Type': 'application/json; charset=utf-8' }
                        });
                    } else if (request.method === 'POST') {
                        // ÊîØÊåÅÈÄöËøá POST Ê∏ÖÁ©∫ÈîôËØØÊó•ÂøóÔºö{"action":"clear"}
                        try {
                            const body = await request.json();
                            if (body && body.action === 'clear') {
                                clearErrorLogs();
                                return new Response(JSON.stringify({
                                    success: true,
                                    message: 'ÈîôËØØÊó•ÂøóÂ∑≤Ê∏ÖÁ©∫'
                                }), {
                                    status: 200,
                                    headers: { 'Content-Type': 'application/json; charset=utf-8' }
                                });
                            }
                        } catch (e) {
                            // ÂøΩÁï•Ëß£ÊûêÈîôËØØÔºåËøîÂõû 400
                        }

                        const resp = {
                            success: false,
                            error: 'Êó†ÊïàÁöÑËØ∑Ê±Ç',
                            message: '‰ªÖÊîØÊåÅ GET Ëé∑ÂèñÊó•ÂøóÔºåÊàñ POST {\"action\":\"clear\"} Ê∏ÖÁ©∫Êó•Âøó'
                        };
                        logHTTPError(400, JSON.stringify(resp), 'ErrorLogsAPI', {
                            url: url.pathname,
                            method: request.method,
                            user: envUKey || 'unknown',
                            workerRegion: currentWorkerRegion || 'unknown'
                        });
                        return new Response(JSON.stringify(resp), {
                            status: 400,
                            headers: { 'Content-Type': 'application/json; charset=utf-8' }
                        });
                    }

                    const resp = {
                        success: false,
                        error: 'ÊñπÊ≥ï‰∏çË¢´ÂÖÅËÆ∏',
                        message: '‰ªÖÊîØÊåÅ GET Âíå POST'
                    };
                    logHTTPError(405, JSON.stringify(resp), 'ErrorLogsAPI', {
                        url: url.pathname,
                        method: request.method,
                        user: envUKey || 'unknown',
                        workerRegion: currentWorkerRegion || 'unknown'
                    });
                    return new Response(JSON.stringify(resp), {
                        status: 405,
                        headers: { 'Content-Type': 'application/json; charset=utf-8' }
                    });
                }

                const resp = { error: 'Êó†ÊïàÁöÑAPIË∑ØÂæÑ' };
                logHTTPError(404, JSON.stringify(resp), 'ErrorLogsAPI', {
                    url: url.pathname,
                    method: request.method,
                    user: envUKey || 'unknown',
                    workerRegion: currentWorkerRegion || 'unknown'
                });
                return new Response(JSON.stringify(resp), {
                    status: 404,
                    headers: { 'Content-Type': 'application/json; charset=utf-8' }
                });
            }

            // ‰ºòÂÖàÂ§ÑÁêÜWebSocketÂçáÁ∫ßËØ∑Ê±ÇÔºàÂøÖÈ°ªÂú®ÂÖ∂‰ªñË∑ØÁî±‰πãÂâçÔºâ
            const upgradeHeader = request.headers.get('Upgrade');
            console.log('[ENTRY-DEBUG] ËØ∑Ê±ÇÂà∞Ëææ - method:', request.method, 'url:', request.url, 'Upgrade header:', upgradeHeader);
            if (upgradeHeader && upgradeHeader.toLowerCase() === atob('d2Vic29ja2V0')) {
                console.log('[ENTRY-DEBUG] WebSocketÂçáÁ∫ßËØ∑Ê±ÇÊ£ÄÊµãÂà∞ÔºåË∞ÉÁî®handleWsRequest');
                return await handleWsRequest(request);
            }

            if (request.method === 'POST' && ex) {
                const r = await handleXhttpPost(request);
                if (r) {
                    ctx.waitUntil(r.closed);
                    return new Response(r.readable, {
                        headers: {
                            'X-Accel-Buffering': 'no',
                            'Cache-Control': 'no-store',
                            Connection: 'keep-alive',
                            'User-Agent': 'Go-http-client/2.0',
                            'Content-Type': 'application/grpc',
                        },
                    });
                }
                logHTTPError(500, 'Internal Server Error - XHTTP POST handler failed', 'MainHandler', {
                    method: request.method,
                    workerRegion: currentWorkerRegion || 'unknown'
                });
                return new Response('Internal Server Error', { status: 500 });
            }

            if (request.method === 'GET') {
                // Â§ÑÁêÜ /{UUID}/region Êàñ /{Ëá™ÂÆö‰πâË∑ØÂæÑ}/region
                if (url.pathname.endsWith('/region')) {
                    const pathParts = url.pathname.split('/').filter(p => p);

                    if (pathParts.length === 2 && pathParts[1] === 'region') {
                        const pathIdentifier = pathParts[0];
                        let isValid = false;

                        if (cp && cp.trim()) {
                            // ‰ΩøÁî®Ëá™ÂÆö‰πâË∑ØÂæÑ
                            const cleanCustomPath = cp.trim().startsWith('/') ? cp.trim().substring(1) : cp.trim();
                            isValid = (pathIdentifier === cleanCustomPath);
                        } else {
                            // È™åËØÅÂøÖÈ°ª‰∏éenvUKeyÂÆåÂÖ®ÂåπÈÖçÔºàÊîØÊåÅÂ§ßÂ∞èÂÜôÔºâ
                            isValid = (pathIdentifier === envUKey);
                        }

                        if (isValid) {
                            const ci = getConfigValue('p', env.p || env.P);
                            const manualRegion = getConfigValue('wk', env.wk || env.WK);

                            if (manualRegion && manualRegion.trim()) {
                                return new Response(JSON.stringify({
                                    region: manualRegion.trim().toUpperCase(),
                                    detectionMethod: 'ÊâãÂä®ÊåáÂÆöÂú∞Âå∫',
                                    manualRegion: manualRegion.trim().toUpperCase(),
                                    timestamp: new Date().toISOString()
                                }), {
                                    headers: { 'Content-Type': 'application/json' }
                                });
                            } else if (ci && ci.trim()) {
                                return new Response(JSON.stringify({
                                    region: 'CUSTOM',
                                    detectionMethod: 'Ëá™ÂÆö‰πâProxyIPÊ®°Âºè', ci: ci,
                                    timestamp: new Date().toISOString()
                                }), {
                                    headers: { 'Content-Type': 'application/json' }
                                });
                            } else {
                                const detectedRegion = await detectWorkerRegion(request);
                                return new Response(JSON.stringify({
                                    region: detectedRegion,
                                    detectionMethod: 'APIÊ£ÄÊµã',
                                    timestamp: new Date().toISOString()
                                }), {
                                    headers: { 'Content-Type': 'application/json' }
                                });
                            }
                        } else {
                            return new Response(JSON.stringify({
                                error: 'ËÆøÈóÆË¢´ÊãíÁªù',
                                message: 'Ë∑ØÂæÑÈ™åËØÅÂ§±Ë¥•'
                            }), {
                                status: 403,
                                headers: { 'Content-Type': 'application/json' }
                            });
                        }
                    }
                }

                // Â§ÑÁêÜ /{UUID}/test-api Êàñ /{Ëá™ÂÆö‰πâË∑ØÂæÑ}/test-api
                if (url.pathname.endsWith('/test-api')) {
                    const pathParts = url.pathname.split('/').filter(p => p);

                    if (pathParts.length === 2 && pathParts[1] === 'test-api') {
                        const pathIdentifier = pathParts[0];
                        let isValid = false;

                        if (cp && cp.trim()) {
                            // ‰ΩøÁî®Ëá™ÂÆö‰πâË∑ØÂæÑ
                            const cleanCustomPath = cp.trim().startsWith('/') ? cp.trim().substring(1) : cp.trim();
                            isValid = (pathIdentifier === cleanCustomPath);
                        } else {
                            // È™åËØÅÂøÖÈ°ª‰∏éenvUKeyÂÆåÂÖ®ÂåπÈÖçÔºàÊîØÊåÅÂ§ßÂ∞èÂÜôÔºâ
                            isValid = (pathIdentifier === envUKey);
                        }

                        if (isValid) {
                            try {
                                const testRegion = await detectWorkerRegion(request);
                                return new Response(JSON.stringify({
                                    detectedRegion: testRegion,
                                    message: 'APIÊµãËØïÂÆåÊàê',
                                    timestamp: new Date().toISOString()
                                }), {
                                    headers: { 'Content-Type': 'application/json' }
                                });
                            } catch (error) {
                                return new Response(JSON.stringify({
                                    error: error.message,
                                    message: 'APIÊµãËØïÂ§±Ë¥•'
                                }), {
                                    status: 500,
                                    headers: { 'Content-Type': 'application/json' }
                                });
                            }
                        } else {
                            return new Response(JSON.stringify({
                                error: 'ËÆøÈóÆË¢´ÊãíÁªù',
                                message: 'Ë∑ØÂæÑÈ™åËØÅÂ§±Ë¥•'
                            }), {
                                status: 403,
                                headers: { 'Content-Type': 'application/json' }
                            });
                        }
                    }
                }

                // ÈîôËØØÊó•ÂøóÈ°µÈù¢Ôºö/{UUID}/error-logs Êàñ /{Ëá™ÂÆö‰πâË∑ØÂæÑ}/error-logs
                if (url.pathname.endsWith('/error-logs')) {
                    const pathParts = url.pathname.split('/').filter(p => p);

                    if (pathParts.length === 2 && pathParts[1] === 'error-logs') {
                        const pathIdentifier = pathParts[0];
                        let isValid = false;

                        if (cp && cp.trim()) {
                            // ‰ΩøÁî®Ëá™ÂÆö‰πâË∑ØÂæÑ
                            const cleanCustomPath = cp.trim().startsWith('/') ? cp.trim().substring(1) : cp.trim();
                            isValid = (pathIdentifier === cleanCustomPath);
                        } else {
                            // È™åËØÅÂøÖÈ°ª‰∏éenvUKeyÂÆåÂÖ®ÂåπÈÖçÔºàÊîØÊåÅÂ§ßÂ∞èÂÜôÔºâ
                            isValid = (pathIdentifier === envUKey);
                        }

                        if (!isValid) {
                            return new Response(JSON.stringify({
                                error: 'ËÆøÈóÆË¢´ÊãíÁªù',
                                message: 'Ë∑ØÂæÑÈ™åËØÅÂ§±Ë¥•'
                            }), {
                                status: 403,
                                headers: { 'Content-Type': 'application/json; charset=utf-8' }
                            });
                        }

                        const html = `<!DOCTYPE html>
<html lang="zh-CN">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>ÈîôËØØÊó•ÂøóÊü•ÁúãÂô®</title>
    <style>
        * { box-sizing: border-box; margin: 0; padding: 0; }
        body {
            font-family: -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, "Helvetica Neue", Arial, sans-serif;
            background: radial-gradient(circle at 0% 0%, rgba(255,255,255,0.96) 0%, transparent 45%),
                        radial-gradient(circle at 100% 100%, rgba(59,130,246,0.12) 0%, transparent 55%),
                        linear-gradient(135deg, #0f172a 0%, #020617 100%);
            color: #e5e7eb;
            min-height: 100vh;
            padding: 24px 12px;
        }
        .container {
            max-width: 1100px;
            margin: 0 auto;
            background: rgba(15,23,42,0.94);
            border-radius: 16px;
            padding: 24px;
            box-shadow: 0 24px 80px rgba(0,0,0,0.55);
            border: 1px solid rgba(148,163,184,0.25);
        }
        .header {
            display: flex;
            justify-content: space-between;
            align-items: center;
            margin-bottom: 18px;
            gap: 12px;
            flex-wrap: wrap;
        }
        .title {
            font-size: 1.6rem;
            font-weight: 600;
            background: linear-gradient(135deg, #e5e7eb 0%, #60a5fa 50%, #34d399 100%);
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
            background-clip: text;
        }
        .subtitle {
            font-size: 0.9rem;
            color: #9ca3af;
            margin-top: 2px;
        }
        .badge {
            display: inline-flex;
            align-items: center;
            gap: 6px;
            padding: 4px 10px;
            border-radius: 999px;
            background: rgba(34,197,94,0.08);
            border: 1px solid rgba(34,197,94,0.35);
            font-size: 0.75rem;
            color: #bbf7d0;
        }
        .controls {
            display: grid;
            grid-template-columns: repeat(auto-fit,minmax(200px,1fr));
            gap: 12px;
            margin-bottom: 16px;
        }
        .field { display: flex; flex-direction: column; gap: 4px; }
        label { font-size: 0.8rem; color: #9ca3af; }
        input, select {
            background: rgba(15,23,42,0.9);
            border-radius: 8px;
            border: 1px solid rgba(55,65,81,0.9);
            padding: 6px 10px;
            color: #e5e7eb;
            font-size: 0.85rem;
            outline: none;
            transition: border-color 0.15s ease, box-shadow 0.15s ease;
        }
        input:focus, select:focus {
            border-color: #3b82f6;
            box-shadow: 0 0 0 1px rgba(59,130,246,0.5);
        }
        .actions {
            display: flex;
            flex-wrap: wrap;
            gap: 8px;
            align-items: center;
            margin-bottom: 10px;
        }
        button {
            border-radius: 999px;
            border: none;
            padding: 6px 14px;
            font-size: 0.8rem;
            font-weight: 500;
            cursor: pointer;
            display: inline-flex;
