        function connectServer(id) {
            const wsUrl = \`wss://${hostname}/rdp/\${id}\`;
            alert(\`WebSocketè¿æ¥åœ°å€ï¼š\\n\${wsUrl}\\n\\nè¯·ä½¿ç”¨RDPå®¢æˆ·ç«¯å·¥å…·è¿æ¥æ­¤åœ°å€\`);
            // è¿™é‡Œå¯ä»¥æ‰“å¼€ä¸€ä¸ªæ–°çª—å£åˆ°å®¢æˆ·ç«¯é¡µé¢
            // window.open('/rdp-client.html?server=' + id, '_blank');
        }
        
        // åˆ é™¤æœåŠ¡å™¨
        async function deleteServer(id) {
            if (!confirm('ç¡®å®šè¦åˆ é™¤è¿™ä¸ªæœåŠ¡å™¨å—ï¼Ÿ')) {
                return;
            }
            
            try {
                const response = await fetch(API_BASE + '-servers/' + id, {
                    method: 'DELETE'
                });
                
                const result = await response.json();
                
                if (response.ok) {
                    alert('âœ… æœåŠ¡å™¨å·²åˆ é™¤');
                    loadServers();
                } else {
                    alert('âŒ åˆ é™¤å¤±è´¥ï¼š' + (result.error || 'æœªçŸ¥é”™è¯¯'));
                }
            } catch (error) {
                alert('âŒ åˆ é™¤å¤±è´¥ï¼š' + error.message);
            }
        }
        
        // é¡µé¢åŠ è½½æ—¶åŠ è½½æœåŠ¡å™¨åˆ—è¡¨
        loadServers();
    </script>
</body>
</html>`;

    return new Response(rdpAdminHTML, {
        headers: { 'Content-Type': 'text/html; charset=utf-8' }
    });
}

/**
 * å¤„ç†RDP APIè¯·æ±‚
 */
async function handleRdpAPI(request) {
    const url = new URL(request.url);
    const path = url.pathname;

    // GET /api/rdp-servers - è·å–æœåŠ¡å™¨åˆ—è¡¨
    if (request.method === 'GET' && path.endsWith('/rdp-servers')) {
        return new Response(JSON.stringify({
            success: true,
            servers: rdpServers
        }), {
            headers: { 'Content-Type': 'application/json; charset=utf-8' }
        });
    }

    // POST /api/rdp-servers - æ·»åŠ æœåŠ¡å™¨
    if (request.method === 'POST' && path.endsWith('/rdp-servers')) {
        try {
            const data = await request.json();

            if (!data.name || !data.host || !data.port) {
                return new Response(JSON.stringify({
                    success: false,
                    error: 'ç¼ºå°‘å¿…è¦å‚æ•°ï¼šname, host, port'
                }), {
                    status: 400,
                    headers: { 'Content-Type': 'application/json; charset=utf-8' }
                });
            }

            const newServer = {
                id: generateRandomId(),
                name: data.name,
                host: data.host,
                port: data.port,
                addedAt: new Date().toISOString()
            };

            rdpServers.push(newServer);

            return new Response(JSON.stringify({
                success: true,
                server: newServer
            }), {
                headers: { 'Content-Type': 'application/json; charset=utf-8' }
            });
        } catch (error) {
            return new Response(JSON.stringify({
                success: false,
                error: error.message
            }), {
                status: 500,
                headers: { 'Content-Type': 'application/json; charset=utf-8' }
            });
        }
    }

    // DELETE /api/rdp-servers/{id} - åˆ é™¤æœåŠ¡å™¨
    if (request.method === 'DELETE' && path.includes('/rdp-servers/')) {
        const serverId = path.split('/rdp-servers/')[1];
        const index = rdpServers.findIndex(s => s.id === serverId);

        if (index === -1) {
            return new Response(JSON.stringify({
                success: false,
                error: 'æœåŠ¡å™¨ä¸å­˜åœ¨'
            }), {
                status: 404,
                headers: { 'Content-Type': 'application/json; charset=utf-8' }
            });
        }

        rdpServers.splice(index, 1);

        return new Response(JSON.stringify({
            success: true,
            message: 'æœåŠ¡å™¨å·²åˆ é™¤'
        }), {
            headers: { 'Content-Type': 'application/json; charset=utf-8' }
        });
    }

    return new Response(JSON.stringify({
        success: false,
        error: 'ä¸æ”¯æŒçš„APIç«¯ç‚¹'
    }), {
        status: 404,
        headers: { 'Content-Type': 'application/json; charset=utf-8' }
    });
}

/**
 * ç”ŸæˆéšæœºID
 */
function generateRandomId() {
    return Math.random().toString(36).substring(2, 15) + Math.random().toString(36).substring(2, 15);
}
