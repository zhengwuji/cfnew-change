        </div>
        
        <div class="section">
            <h2 class="section-title">üìù ËØ¥Êòé</h2>
            <div class="endpoint">
                <div class="endpoint-desc">
                    ‚Ä¢ Â∏¶Êúâ<span class="badge">ÈúÄË¶ÅUUID</span>Ê†áËÆ∞ÁöÑÁ´ØÁÇπÈúÄË¶ÅÂ∞Ü {UUID} ÊõøÊç¢‰∏∫‰Ω†ÁöÑÂÆûÈôÖUUID<br>
                    ‚Ä¢ ÊâÄÊúâÁ´ØÁÇπÂùáÊîØÊåÅHTTPSËÆøÈóÆ<br>
                    ‚Ä¢ APIËøîÂõûÊ†ºÂºè: JSON
                </div>
            </div>
        </div>
    </div>
</body>
</html>`;
                    return new Response(apiDocsHTML, {
                        headers: { 'Content-Type': 'text/html; charset=utf-8' }
                    });
                }

                // ÈîôËØØÊó•ÂøóÊü•ÁúãÈ°µÈù¢Ë∑ØÁî±
                if (url.pathname === '/error-logs' || url.pathname === '/error-logs/') {
                    return await handleErrorLogsPage(request, url);
                }

                // ÈîôËØØÊó•ÂøóAPIÁ´ØÁÇπÔºàJSONÊ†ºÂºèÔºâ
                if (url.pathname === '/api/error-logs') {
                    return await handleErrorLogsAPI(request, url);
                }



                if (url.pathname === '/') {
                    // Ê£ÄÊü•ÊòØÂê¶ÊúâËá™ÂÆö‰πâÈ¶ñÈ°µURLÈÖçÁΩÆ
                    const customHomepage = getConfigValue('homepage', env.homepage || env.HOMEPAGE);
                    if (customHomepage && customHomepage.trim()) {
                        try {
                            // ‰ªéËá™ÂÆö‰πâURLËé∑ÂèñÂÜÖÂÆπ
                            const homepageResponse = await fetch(customHomepage.trim(), {
                                method: 'GET',
                                headers: {
                                    'User-Agent': request.headers.get('User-Agent') || 'Mozilla/5.0',
                                    'Accept': request.headers.get('Accept') || '*/*',
                                    'Accept-Language': request.headers.get('Accept-Language') || 'en-US,en;q=0.9',
                                },
                                redirect: 'follow'
                            });

                            if (homepageResponse.ok) {
                                // Ëé∑ÂèñÂìçÂ∫îÂÜÖÂÆπ
                                const contentType = homepageResponse.headers.get('Content-Type') || 'text/html; charset=utf-8';
                                const content = await homepageResponse.text();

                                // ËøîÂõûËá™ÂÆö‰πâÈ¶ñÈ°µÂÜÖÂÆπ
                                return new Response(content, {
                                    status: homepageResponse.status,
                                    headers: {
                                        'Content-Type': contentType,
                                        'Cache-Control': 'no-cache, no-store, must-revalidate',
                                    }
                                });
                            }
                        } catch (error) {
                            // Â¶ÇÊûúËé∑ÂèñÂ§±Ë¥•ÔºåÁªßÁª≠‰ΩøÁî®ÈªòËÆ§ÁªàÁ´ØÈ°µÈù¢
                            console.error('Ëé∑ÂèñËá™ÂÆö‰πâÈ¶ñÈ°µÂ§±Ë¥•:', error);
                        }
                    }
                    // Âõ∫ÂÆö‰ΩøÁî®‰∏≠Êñá
                    const translations = {
                        title: 'ÁªàÁ´Ø',
                        terminal: 'ÁªàÁ´Ø',
                        congratulations: 'ÊÅ≠Âñú‰Ω†Êù•Âà∞Ëøô',
                        enterU: 'ËØ∑ËæìÂÖ•‰Ω†UÂèòÈáèÁöÑÂÄº',
                        enterD: 'ËØ∑ËæìÂÖ•‰Ω†DÂèòÈáèÁöÑÂÄº',
                        command: 'ÂëΩ‰ª§: connect [',
                        uuid: 'UUID',
                        path: 'PATH',
                        inputU: 'ËæìÂÖ•UÂèòÈáèÁöÑÂÜÖÂÆπÂπ∂‰∏îÂõûËΩ¶...',
                        inputD: 'ËæìÂÖ•DÂèòÈáèÁöÑÂÜÖÂÆπÂπ∂‰∏îÂõûËΩ¶...',
                        connecting: 'Ê≠£Âú®ËøûÊé•...',
                        invading: 'Ê≠£Âú®ÂÖ•‰æµ...',
                        success: 'ËøûÊé•ÊàêÂäüÔºÅËøîÂõûÁªìÊûú...',
                        error: 'ÈîôËØØ: Êó†ÊïàÁöÑUUIDÊ†ºÂºè',
                        reenter: 'ËØ∑ÈáçÊñ∞ËæìÂÖ•ÊúâÊïàÁöÑUUID'
                    };

                    const t = translations;

                    const terminalHtml = `<!DOCTYPE html>
        <html lang="zh-CN" dir="ltr">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
            <title>${t.title}</title>
        <style>
            * { margin: 0; padding: 0; box-sizing: border-box; }
            body {
                font-family: "Courier New", monospace;
                background: #000; color: #00ff00; min-height: 100vh;
                overflow-x: hidden; position: relative;
                display: flex; justify-content: center; align-items: center;
            }
            .matrix-bg {
                position: fixed; top: 0; left: 0; width: 100%; height: 100%;
                background: 
                    radial-gradient(circle at 20% 50%, rgba(0, 80, 0, 0.3) 0%, transparent 50%),
                    radial-gradient(circle at 80% 80%, rgba(0, 120, 0, 0.2) 0%, transparent 50%),
                    linear-gradient(135deg, #000000 0%, #001a00 25%, #000000 50%, #002200 75%, #000000 100%);
                background-size: 100% 100%;
