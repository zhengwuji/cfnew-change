                    alert('âŒ æ·»åŠ å¤±è´¥ï¼š' + (result.error || 'æœªçŸ¥é”™è¯¯'));
                }
            } catch (error) {
                alert('âŒ æ·»åŠ å¤±è´¥ï¼š' + error.message);
            }
        });
        
