                                    yxURL: document.getElementById('yxURL').value,
                                    socksConfig: document.getElementById('socksConfig').value
                                };
                                await saveConfig(configData);
                                showStatus('å·²æ›¿æ¢ ' + selectedItems.length + ' é¡¹å¹¶å·²ä¿å­˜', 'success');
                            } catch (err) {
                                showStatus('ä¿å­˜å¤±è´¥: ' + err.message, 'error');
                            } finally {
                                addSelectedBtn.disabled = false;
                                addSelectedBtn.textContent = 'âœ“ æ·»åŠ é€‰ä¸­é¡¹åˆ°ä¼˜é€‰åˆ—è¡¨';
                            }
                        }
                    });
                }
                
                function ipToHex(ip) {
                    const parts = ip.split('.');
                    if (parts.length !== 4) return null;
                    let hex = '';
                    for (let i = 0; i < 4; i++) {
                        const num = parseInt(parts[i]);
                        if (isNaN(num) || num < 0 || num > 255) return null;
                        hex += num.toString(16).padStart(2, '0');
                    }
                    return hex;
                }
                
                const coloMap = {
                    'SJC': 'åœ£ä½•å¡', 'LAX': 'æ´›æ‰çŸ¶', 'SEA': 'è¥¿é›…å›¾', 'SFO': 'æ—§é‡‘å±±', 'DFW': 'è¾¾æ‹‰æ–¯',
