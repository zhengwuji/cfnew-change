                        urlFetchDiv.style.display = savedSource === 'urlFetch' ? 'block' : 'none';
                        cfRandomDiv.style.display = savedSource === 'cfRandom' ? 'block' : 'none';
                        randomCountDiv.style.display = savedSource === 'cfRandom' ? 'block' : 'none';
                    }
