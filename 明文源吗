    <script>
        (function() {
            const tbody = document.getElementById('log-body');
            const statusText = document.getElementById('status-text');
            const statusCount = document.getElementById('status-count');
            const statusUpdated = document.getElementById('status-updated');
            const btnRefresh = document.getElementById('btn-refresh');
            const btnLoadMore = document.getElementById('btn-load-more');
            const btnClear = document.getElementById('btn-clear');
            const autoRefresh = document.getElementById('auto-refresh');
            const contextInput = document.getElementById('context');
            const keywordInput = document.getElementById('keyword');
            const sinceInput = document.getElementById('since');
            const limitInput = document.getElementById('limit');

            let offset = 0;
            let loading = false;
            let lastTotal = 0;
            let timer = null;

            function getApiBase() {
                const parts = window.location.pathname.split('/').filter(Boolean);
                if (parts.length >= 2) {
                    return '/' + parts[0] + '/api/error-logs';
                }
                // ÂÖúÂ∫ïÔºöÂΩìÂâçË∑ØÂæÑÂéªÊéâÊúÄÂêé‰∏ÄÊÆµ
                return window.location.pathname.replace(/\\/error-logs\\/?$/, '') + '/api/error-logs';
            }

            function formatDate(str) {
                if (!str) return '-';
                try {
                    const d = new Date(str);
                    if (isNaN(d.getTime())) return str;
                    return d.toISOString().replace('T',' ').replace('Z','');
                } catch (e) {
                    return str;
                }
            }

            function escapeHtml(str) {
                if (str == null) return '';
                return String(str)
                    .replace(/&/g,'&amp;')
                    .replace(/</g,'&lt;')
                    .replace(/>/g,'&gt;')
                    .replace(/\"/g,'&quot;')
                    .replace(/'/g,'&#39;');
            }

            function renderLogs(logs, append) {
                if (!append) {
                    tbody.innerHTML = '';
                }
                if (!logs || logs.length === 0) {
                    if (!append) {
                        tbody.innerHTML = '<tr><td colspan="5" class="empty">ÊöÇÊó†ÈîôËØØÊó•ÂøóÔºåÁ≥ªÁªüËøêË°å‰∏ÄÂàáÊ≠£Â∏∏ üéâ</td></tr>';
                    }
                    return;
                }

                const rows = logs.map(log => {
                    const status = log.statusCode || '';
                    let statusClass = 'info';
                    if (status >= 500) statusClass = 'error';
                    else if (status >= 400) statusClass = 'warn';
                    const statusText = status ? ('HTTP ' + status) : 'ËøêË°åÊó∂ÈîôËØØ';
                    const region = log.workerRegion || '-';
                    const url = log.url || '-';
                    const method = log.method || '';
                    const user = log.user || '';
                    const msg = log.message || '';
                    const ctx = log.context || '';
                    const stack = log.stack || '';

                    return '<tr>' +
                        '<td>' + escapeHtml(formatDate(log.timestamp)) + '</td>' +
                        '<td><div class="log-meta">' + escapeHtml(ctx) + '</div></td>' +
                        '<td>' +
                            '<div class="badge-status ' + statusClass + '">' + escapeHtml(statusText) + '</div>' +
                            '<div class="log-meta" style="margin-top:4px;">Âú∞Âå∫: ' + escapeHtml(region) + '</div>' +
                        '</td>' +
                        '<td>' +
                            '<div class="log-meta">URL: ' + escapeHtml(url) + '</div>' +
                            '<div class="log-meta">ÊñπÊ≥ï: ' + escapeHtml(method) + '</div>' +
                            '<div class="log-meta">Áî®Êà∑/ÂØÜÈí•: ' + escapeHtml(user) + '</div>' +
                        '</td>' +
                        '<td>' +
                            '<div class="log-message">' + escapeHtml(msg) + '</div>' +
                            (stack ? '<pre class="log-meta" style="margin-top:4px; white-space:pre-wrap; max-height:160px; overflow:auto;">' + escapeHtml(stack) + '</pre>' : '') +
                        '</td>' +
                    '</tr>';
                }).join('');

                if (append) {
                    tbody.insertAdjacentHTML('beforeend', rows);
                } else {
                    tbody.innerHTML = rows;
                }
            }

            async function fetchLogs(append) {
                if (loading) return;
                loading = true;
                statusText.textContent = 'Ê≠£Âú®Âä†ËΩΩÊó•Âøó...';

                const limit = Math.min(Math.max(parseInt(limitInput.value || '200', 10) || 200, 1), 500);
                const params = new URLSearchParams();
                params.set('limit', limit.toString());
                params.set('offset', append ? offset.toString() : '0');
                if (contextInput.value.trim()) params.set('context', contextInput.value.trim());
                if (keywordInput.value.trim()) params.set('message', keywordInput.value.trim());
                if (sinceInput.value.trim()) params.set('since', sinceInput.value.trim());

                try {
                    const res = await fetch(getApiBase() + '?' + params.toString(), {
                        method: 'GET',
                        headers: { 'Accept': 'application/json' }
                    });
                    if (!res.ok) {
                        statusText.textContent = 'Âä†ËΩΩÂ§±Ë¥•: HTTP ' + res.status;
                        loading = false;
                        return;
                    }
                    const data = await res.json();
                    const logs = data.logs || [];
                    if (!append) offset = 0;
                    offset += logs.length;
                    lastTotal = (append ? lastTotal + logs.length : logs.length);
                    renderLogs(logs, append);
                    statusCount.textContent = lastTotal + ' Êù°Êó•Âøó';
                    statusText.textContent = logs.length ? 'Âä†ËΩΩÂÆåÊàê' : 'ÊöÇÊó†Êñ∞Êó•Âøó';
                    statusUpdated.textContent = '‰∏äÊ¨°Êõ¥Êñ∞: ' + formatDate(new Date().toISOString());
                } catch (e) {
                    console.error('Âä†ËΩΩÈîôËØØÊó•ÂøóÂ§±Ë¥•', e);
                    statusText.textContent = 'Âä†ËΩΩÂ§±Ë¥•: ' + (e && e.message ? e.message : 'Êú™Áü•ÈîôËØØ');
                } finally {
                    loading = false;
                }
            }

            async function clearLogs() {
                if (!confirm('Á°ÆÂÆöË¶ÅÊ∏ÖÁ©∫ÂΩìÂâçÂÆû‰æãÂÜÖÂ≠ò‰∏≠ÁöÑÈîôËØØÊó•ÂøóÂêóÔºü\\nÊ≠§Êìç‰Ωú‰ªÖÂΩ±ÂìçÂΩìÂâç Worker ÂÆû‰æãÔºåÊó†Ê≥ïÊÅ¢Â§ç„ÄÇ')) return;
                try {
                    const res = await fetch(getApiBase(), {
                        method: 'POST',
                        headers: { 'Content-Type': 'application/json' },
                        body: JSON.stringify({ action: 'clear' })
                    });
                    const data = await res.json().catch(() => ({}));
                    if (res.ok && data.success) {
                        offset = 0;
                        lastTotal = 0;
                        renderLogs([], false);
                        statusCount.textContent = '0 Êù°Êó•Âøó';
                        statusText.textContent = 'Êó•ÂøóÂ∑≤Ê∏ÖÁ©∫';
                    } else {
                        statusText.textContent = 'Ê∏ÖÁ©∫Â§±Ë¥•: ' + (data && data.message ? data.message : 'Êú™Áü•ÂéüÂõ†');
                    }
                } catch (e) {
                    console.error('Ê∏ÖÁ©∫Êó•ÂøóÂ§±Ë¥•', e);
                    statusText.textContent = 'Ê∏ÖÁ©∫Â§±Ë¥•: ' + (e && e.message ? e.message : 'Êú™Áü•ÈîôËØØ');
                }
            }

            function setupAutoRefresh() {
                if (timer) {
                    clearInterval(timer);
                    timer = null;
                }
                if (autoRefresh.checked) {
                    timer = setInterval(() => {
                        fetchLogs(false);
                    }, 5000);
                }
            }

            btnRefresh.addEventListener('click', () => {
                offset = 0;
                lastTotal = 0;
                fetchLogs(false);
            });

            btnLoadMore.addEventListener('click', () => {
                fetchLogs(true);
            });

            btnClear.addEventListener('click', () => {
                clearLogs();
            });

            autoRefresh.addEventListener('change', () => {
                setupAutoRefresh();
            });

            // ÂàùÂßãÂä†ËΩΩ
            fetchLogs(false);
            setupAutoRefresh();
        })();
    </script>
</body>
</html>`;

                        return new Response(html, {
                            status: 200,
                            headers: {
                                'Content-Type': 'text/html; charset=utf-8',
                                'Cache-Control': 'no-store'
                            }
                        });
                    }
                }

                // APIÊñáÊ°£È°µÈù¢Ë∑ØÁî±
                if (url.pathname === '/api-docs' || url.pathname === '/api-docs/') {
                    const hostname = url.hostname;
                    const apiDocsHTML = `<!DOCTYPE html>
<html lang="zh-CN">
<head>
