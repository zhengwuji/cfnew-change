        </div>
        <script>
            // ËÆ¢ÈòÖËΩ¨Êç¢Âú∞ÂùÄÔºà‰ªéÊúçÂä°Âô®ÈÖçÁΩÆÊ≥®ÂÖ•Ôºâ
            var SUB_CONVERTER_URL = "${scu}";
            // ËøúÁ®ãÈÖçÁΩÆURLÔºàÁ°¨ÁºñÁ†ÅÔºâ
            var REMOTE_CONFIG_URL = "${remoteConfigUrl}";
            
                // ÁøªËØëÂØπË±°
                const translations = {
                    subscriptionCopied: 'ËÆ¢ÈòÖÈìæÊé•Â∑≤Â§çÂà∂',
                    autoSubscriptionCopied: 'Ëá™Âä®ËØÜÂà´ËÆ¢ÈòÖÈìæÊé•Â∑≤Â§çÂà∂ÔºåÂÆ¢Êà∑Á´ØËÆøÈóÆÊó∂‰ºöÊ†πÊçÆUser-AgentËá™Âä®ËØÜÂà´Âπ∂ËøîÂõûÂØπÂ∫îÊ†ºÂºè'
                };
                
                function getCookie(name) {
                    const value = '; ' + document.cookie;
                    const parts = value.split('; ' + name + '=');
                    if (parts.length === 2) return parts.pop().split(';').shift();
                    return null;
                }
                
                const t = translations;
            
            function tryOpenApp(schemeUrl, fallbackCallback, timeout) {
                timeout = timeout || 2500;
                var appOpened = false;
                var callbackExecuted = false;
                var startTime = Date.now();
                
                var blurHandler = function() {
                    var elapsed = Date.now() - startTime;
                    if (elapsed < 3000 && !callbackExecuted) {
                        appOpened = true;
                    }
                };
                
                window.addEventListener('blur', blurHandler);
                
                var hiddenHandler = function() {
                    var elapsed = Date.now() - startTime;
                    if (elapsed < 3000 && !callbackExecuted) {
                        appOpened = true;
                    }
                };
                
                document.addEventListener('visibilitychange', hiddenHandler);
                
                var iframe = document.createElement('iframe');
                iframe.style.display = 'none';
                iframe.style.width = '1px';
                iframe.style.height = '1px';
                iframe.src = schemeUrl;
                document.body.appendChild(iframe);
                
                setTimeout(function() {
                    iframe.parentNode && iframe.parentNode.removeChild(iframe);
                    window.removeEventListener('blur', blurHandler);
                    document.removeEventListener('visibilitychange', hiddenHandler);
                    
                    if (!callbackExecuted) {
                        callbackExecuted = true;
                        if (!appOpened && fallbackCallback) {
                            fallbackCallback();
                        }
                    }
                }, timeout);
            }
            
            function generateClientLink(clientType, clientName) {
                // ‰ΩøÁî®originÂíåpathnameÊûÑÂª∫ËÆ¢ÈòÖURLÔºåÁ°Æ‰øùË∑ØÂæÑÊ≠£Á°Æ
                var origin = window.location.origin;
                var pathname = window.location.pathname;
                // ÁßªÈô§Êú´Â∞æÁöÑÊñúÊù†ÔºàÂ¶ÇÊûúÊúâÔºâ
                if (pathname.endsWith('/') && pathname.length > 1) {
                    pathname = pathname.slice(0, -1);
                }
                // ÊûÑÂª∫ËÆ¢ÈòÖURL
                var subscriptionUrl = origin + pathname + "/sub";
                var schemeUrl = '';
                var displayName = clientName || '';
                var finalUrl = subscriptionUrl;
                
                if (clientType === atob('djJyYXk=')) {
                    finalUrl = subscriptionUrl;
                    var urlElement = document.getElementById("clientSubscriptionUrl");
                    urlElement.textContent = finalUrl;
                    urlElement.style.display = "block";
                    urlElement.style.overflowWrap = "break-word";
                    urlElement.style.wordBreak = "break-all";
                    urlElement.style.overflowX = "auto";
                    urlElement.style.maxWidth = "100%";
                    urlElement.style.boxSizing = "border-box";
                    
                    if (clientName === 'V2RAY') {
                        navigator.clipboard.writeText(finalUrl).then(function() {
                                alert(displayName + " " + t.subscriptionCopied);
                        }).catch(function(err) {
                            alert("ËÆ¢ÈòÖÈìæÊé•: " + finalUrl + "\\n\\nËØ∑ÊâãÂä®Â§çÂà∂");
                        });
                    } else if (clientName === 'Shadowrocket') {
                        schemeUrl = 'shadowrocket://add/' + encodeURIComponent(finalUrl);
                        tryOpenApp(schemeUrl, function() {
                            navigator.clipboard.writeText(finalUrl).then(function() {
                                    alert(displayName + " " + t.subscriptionCopied);
                            });
                        });
                    } else if (clientName === 'V2RAYNG') {
                        schemeUrl = 'v2rayng://install?url=' + encodeURIComponent(finalUrl);
                        tryOpenApp(schemeUrl, function() {
                            navigator.clipboard.writeText(finalUrl).then(function() {
                                    alert(displayName + " " + t.subscriptionCopied);
                            });
                        });
                    } else if (clientName === 'NEKORAY') {
                        schemeUrl = 'nekoray://install-config?url=' + encodeURIComponent(finalUrl);
                        tryOpenApp(schemeUrl, function() {
                            navigator.clipboard.writeText(finalUrl).then(function() {
                                    alert(displayName + " " + t.subscriptionCopied);
                            });
                        });
                    }
                } else {
                    var encodedUrl = encodeURIComponent(subscriptionUrl);
                    finalUrl = SUB_CONVERTER_URL + "?target=" + clientType + "&url=" + encodedUrl + "&insert=false&config=" + encodeURIComponent(REMOTE_CONFIG_URL) + "&emoji=true&list=false&xudp=false&udp=false&tfo=false&expand=true&scv=false&fdn=false&new_name=true";
                    var urlElement = document.getElementById("clientSubscriptionUrl");
                    urlElement.textContent = finalUrl;
                    urlElement.style.display = "block";
                    urlElement.style.overflowWrap = "break-word";
                    urlElement.style.wordBreak = "break-all";
                    urlElement.style.overflowX = "auto";
                    urlElement.style.maxWidth = "100%";
                    urlElement.style.boxSizing = "border-box";
                    
                    if (clientType === atob('Y2xhc2g=')) {
                        if (clientName === 'STASH') {
                            schemeUrl = 'stash://install?url=' + encodeURIComponent(finalUrl);
                            displayName = 'STASH';
                        } else {
                            schemeUrl = 'clash://install-config?url=' + encodeURIComponent(finalUrl);
                            displayName = 'CLASH';
                        }
                    } else if (clientType === atob('c3VyZ2U=')) {
                        schemeUrl = 'surge:///install-config?url=' + encodeURIComponent(finalUrl);
                        displayName = 'SURGE';
                    } else if (clientType === atob('c2luZ2JveA==')) {
                        schemeUrl = 'sing-box://install-config?url=' + encodeURIComponent(finalUrl);
                        displayName = 'SING-BOX';
                    } else if (clientType === atob('bG9vbg==')) {
                        schemeUrl = 'loon://install?url=' + encodeURIComponent(finalUrl);
                        displayName = 'LOON';
                    } else if (clientType === atob('cXVhbng=')) {
                        schemeUrl = 'quantumult-x://install-config?url=' + encodeURIComponent(finalUrl);
                        displayName = 'QUANTUMULT X';
                    }
                    
                    if (schemeUrl) {
                        tryOpenApp(schemeUrl, function() {
                            navigator.clipboard.writeText(finalUrl).then(function() {
                                    alert(displayName + " " + t.subscriptionCopied);
                            });
                        });
                    } else {
                        navigator.clipboard.writeText(finalUrl).then(function() {
                                alert(displayName + " " + t.subscriptionCopied);
                        }).catch(function(err) {
                            alert("ËÆ¢ÈòÖÈìæÊé•: " + finalUrl + "\\n\\nËØ∑ÊâãÂä®Â§çÂà∂");
                        });
                    }
                }
            }
            
            function createMatrixRain() {
                const matrixContainer = document.getElementById('matrixCodeRain');
                const matrixChars = '01ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz';
                const columns = Math.floor(window.innerWidth / 18);
                
                for (let i = 0; i < columns; i++) {
                    const column = document.createElement('div');
                    column.className = 'matrix-column';
                    column.style.left = (i * 18) + 'px';
                    column.style.animationDelay = Math.random() * 15 + 's';
                    column.style.animationDuration = (Math.random() * 15 + 8) + 's';
                    column.style.fontSize = (Math.random() * 4 + 12) + 'px';
                    column.style.opacity = Math.random() * 0.8 + 0.2;
                    
                    let text = '';
                    const charCount = Math.floor(Math.random() * 30 + 20);
                    for (let j = 0; j < charCount; j++) {
                        const char = matrixChars[Math.floor(Math.random() * matrixChars.length)];
                        const brightness = Math.random() > 0.1 ? '#00ff00' : '#00aa00';
                        text += '<span style="color: ' + brightness + ';">' + char + '</span><br>';
                    }
                    column.innerHTML = text;
                    matrixContainer.appendChild(column);
                }
                
                setInterval(function() {
                    const columns = matrixContainer.querySelectorAll('.matrix-column');
                    columns.forEach(function(column) {
                        if (Math.random() > 0.95) {
                            const chars = column.querySelectorAll('span');
                            if (chars.length > 0) {
                                const randomChar = chars[Math.floor(Math.random() * chars.length)];
                                randomChar.style.color = '#ffffff';
                                setTimeout(function() {
                                    randomChar.style.color = '#00ff00';
                                }, 200);
                            }
                        }
                    });
                }, 100);
            }
            
            async function checkSystemStatus() {
                try {
                    const cfStatus = document.getElementById('cfStatus');
                    const regionStatus = document.getElementById('regionStatus');
                    const geoInfo = document.getElementById('geoInfo');
                    const backupStatus = document.getElementById('backupStatus');
                    const currentIP = document.getElementById('currentIP');
                    const regionMatch = document.getElementById('regionMatch');
                    
                    // Ëé∑ÂèñÁä∂ÊÄÅÁÇπÂÖÉÁ¥†
                    const regionDot = document.getElementById('regionDot');
                    const geoDot = document.getElementById('geoDot');
                    const backupDot = document.getElementById('backupDot');
                    const currentIPDot = document.getElementById('currentIPDot');
                    const regionMatchDot = document.getElementById('regionMatchDot');
                    
                        // Ëé∑ÂèñÂΩìÂâçËØ≠Ë®ÄËÆæÁΩÆÔºà‰ºòÂÖà‰ªéCookie/localStorageËØªÂèñÔºâ
                        function getCookie(name) {
                            const value = '; ' + document.cookie;
                            const parts = value.split('; ' + name + '=');
                            if (parts.length === 2) return parts.pop().split(';').shift();
                            return null;
                        }
                        
                        const translations = {
                            workerRegion: 'WorkerÂú∞Âå∫: ',
                            detectionMethod: 'Ê£ÄÊµãÊñπÂºè: ',
                            proxyIPStatus: 'ProxyIPÁä∂ÊÄÅ: ',
                            currentIP: 'ÂΩìÂâç‰ΩøÁî®IP: ',
                            regionMatch: 'Âú∞Âå∫ÂåπÈÖç: ',
                            regionNames: {
                                'US': 'üá∫üá∏ ÁæéÂõΩ', 'SG': 'üá∏üá¨ Êñ∞Âä†Âù°', 'JP': 'üáØüáµ Êó•Êú¨',
                                'KR': 'üá∞üá∑ Èü©ÂõΩ', 'DE': 'üá©üá™ Âæ∑ÂõΩ', 'SE': 'üá∏üá™ ÁëûÂÖ∏', 'NL': 'üá≥üá± Ëç∑ÂÖ∞',
                                'FI': 'üá´üáÆ Ëä¨ÂÖ∞', 'GB': 'üá¨üáß Ëã±ÂõΩ'
                            },
                            customIPMode: 'Ëá™ÂÆö‰πâProxyIPÊ®°Âºè (pÂèòÈáèÂêØÁî®)',
                            customIPModeDesc: 'Ëá™ÂÆö‰πâIPÊ®°Âºè (Â∑≤Á¶ÅÁî®Âú∞Âå∫ÂåπÈÖç)',
                            usingCustomProxyIP: '‰ΩøÁî®Ëá™ÂÆö‰πâProxyIP: ',
                            customIPConfig: ' (pÂèòÈáèÈÖçÁΩÆ)',
                            customIPModeDisabled: 'Ëá™ÂÆö‰πâIPÊ®°ÂºèÔºåÂú∞Âå∫ÈÄâÊã©Â∑≤Á¶ÅÁî®',
                            manualRegion: 'ÊâãÂä®ÊåáÂÆöÂú∞Âå∫',
                            manualRegionDesc: ' (ÊâãÂä®ÊåáÂÆö)',
                            proxyIPAvailable: '10/10 ÂèØÁî® (ProxyIPÂüüÂêçÈ¢ÑËÆæÂèØÁî®)',
                            smartSelection: 'Êô∫ËÉΩÂ∞±ËøëÈÄâÊã©‰∏≠',
                            sameRegionIP: 'ÂêåÂú∞Âå∫IPÂèØÁî® (1‰∏™)',
                            cloudflareDetection: 'CloudflareÂÜÖÁΩÆÊ£ÄÊµã',
                            detectionFailed: 'Ê£ÄÊµãÂ§±Ë¥•',
                            unknown: 'Êú™Áü•'
                        };
                        
                        const t = translations;
                    
                    let detectedRegion = 'US'; // ÈªòËÆ§ÂÄº
                    let isCustomIPMode = false;
                    let isManualRegionMode = false;
                    let hasError = false;
                    try {
                        const response = await fetch(window.location.pathname + '/region');
                        const data = await response.json();
                        
                        if (data.region === 'CUSTOM') {
                            isCustomIPMode = true;
                            detectedRegion = 'CUSTOM';
                            
                            // Ëé∑ÂèñËá™ÂÆö‰πâIPÁöÑËØ¶ÁªÜ‰ø°ÊÅØ
                                const customIPInfo = data.ci || t.unknown;
                            
                                geoInfo.innerHTML = t.detectionMethod + '<span style="color: #f59e0b;">‚öôÔ∏è ' + t.customIPMode + '</span>';
                                regionStatus.innerHTML = t.workerRegion + '<span style="color: #f59e0b;">üîß ' + t.customIPModeDesc + '</span>';
                            
                            // Êõ¥Êñ∞Áä∂ÊÄÅÁÇπ
                                if (regionDot) regionDot.style.background = '#f59e0b';
                                if (geoDot) geoDot.style.background = '#f59e0b';
                            
                            // ÊòæÁ§∫Ëá™ÂÆö‰πâIPÈÖçÁΩÆÁä∂ÊÄÅÔºåÂåÖÂê´ÂÖ∑‰ΩìIP
                                if (backupStatus) backupStatus.innerHTML = t.proxyIPStatus + '<span style="color: #f59e0b;">üîß ' + t.usingCustomProxyIP + customIPInfo + '</span>';
                                if (currentIP) currentIP.innerHTML = t.currentIP + '<span style="color: #22c55e;">‚úÖ ' + customIPInfo + t.customIPConfig + '</span>';
                                if (regionMatch) regionMatch.innerHTML = t.regionMatch + '<span style="color: #f59e0b;">‚ö†Ô∏è ' + t.customIPModeDisabled + '</span>';
                            
                            // Êõ¥Êñ∞Áä∂ÊÄÅÁÇπ
                                if (backupDot) backupDot.style.background = '#f59e0b';
                                if (currentIPDot) currentIPDot.style.background = '#22c55e';
                                if (regionMatchDot) regionMatchDot.style.background = '#f59e0b';
                            
                            return; // ÊèêÂâçËøîÂõûÔºå‰∏çÊâßË°åÂêéÁª≠ÁöÑÂú∞Âå∫ÂåπÈÖçÈÄªËæë
                            } else if (data.detectionMethod === 'ÊâãÂä®ÊåáÂÆöÂú∞Âå∫' || data.detectionMethod === 'ÿ™ÿπ€å€åŸÜ ŸÖŸÜÿ∑ŸÇŸá ÿØÿ≥ÿ™€å') {
                            isManualRegionMode = true;
                            detectedRegion = data.region;
                            
                                geoInfo.innerHTML = t.detectionMethod + '<span style="color: #666;">' + t.manualRegion + '</span>';
                                regionStatus.innerHTML = t.workerRegion + '<span style="color: #22c55e;">üéØ ' + t.regionNames[detectedRegion] + t.manualRegionDesc + '</span>';
                            
                            // Êõ¥Êñ∞Áä∂ÊÄÅÁÇπ
                                if (regionDot) regionDot.style.background = '#22c55e';
                                if (geoDot) geoDot.style.background = '#22c55e';
                            
                            // ÊòæÁ§∫ÈÖçÁΩÆÁä∂ÊÄÅËÄå‰∏çÊòØÊ£ÄÊµãÁä∂ÊÄÅ
                                if (backupStatus) backupStatus.innerHTML = t.proxyIPStatus + '<span style="color: #22c55e;">‚úÖ ' + t.proxyIPAvailable + '</span>';
                                if (currentIP) currentIP.innerHTML = t.currentIP + '<span style="color: #22c55e;">‚úÖ ' + t.smartSelection + '</span>';
                                if (regionMatch) regionMatch.innerHTML = t.regionMatch + '<span style="color: #22c55e;">‚úÖ ' + t.sameRegionIP + '</span>';
                            
                            // Êõ¥Êñ∞Áä∂ÊÄÅÁÇπ
                                if (backupDot) backupDot.style.background = '#22c55e';
                                if (currentIPDot) currentIPDot.style.background = '#22c55e';
                                if (regionMatchDot) regionMatchDot.style.background = '#22c55e';
                            
                            return; // ÊèêÂâçËøîÂõûÔºå‰∏çÊâßË°åÂêéÁª≠ÁöÑÂú∞Âå∫ÂåπÈÖçÈÄªËæë
                            } else if (data.region && t.regionNames[data.region]) {
                            detectedRegion = data.region;
                        }
                        
                            geoInfo.innerHTML = t.detectionMethod + '<span style="color: #666;">' + t.cloudflareDetection + '</span>';
                            if (geoDot) geoDot.style.background = '#22c55e';
                        
                    } catch (e) {
                            hasError = true;
                            geoInfo.innerHTML = t.detectionMethod + '<span style="color: #ef4444;">' + t.detectionFailed + '</span>';
                            if (geoDot) geoDot.style.background = '#ef4444';
                    }
                    
                        regionStatus.innerHTML = t.workerRegion + '<span style="color: #22c55e;">‚úÖ ' + t.regionNames[detectedRegion] + '</span>';
                        if (regionDot) regionDot.style.background = '#22c55e';
                    
                    // Áõ¥Êé•ÊòæÁ§∫ÈÖçÁΩÆÁä∂ÊÄÅÔºå‰∏çÂÜçËøõË°åÊ£ÄÊµã
                    if (backupStatus) {
                            backupStatus.innerHTML = t.proxyIPStatus + '<span style="color: #22c55e;">‚úÖ ' + t.proxyIPAvailable + '</span>';
                            document.getElementById('backupTag').style.display = 'inline-block';
                            if (backupDot) backupDot.style.background = '#22c55e';
                    }
                    
                    if (currentIP) {
                            currentIP.innerHTML = t.currentIP + '<span style="color: #22c55e;">‚úÖ ' + t.smartSelection + '</span>';
                            if (currentIPDot) currentIPDot.style.background = '#22c55e';
                    }
                    
                    if (regionMatch) {
                            regionMatch.innerHTML = t.regionMatch + '<span style="color: #22c55e;">‚úÖ ' + t.sameRegionIP + '</span>';
                            if (regionMatchDot) regionMatchDot.style.background = '#22c55e';
                    }
                    
                } catch (error) {
                        function getCookie(name) {
                            const value = '; ' + document.cookie;
                            const parts = value.split('; ' + name + '=');
                            if (parts.length === 2) return parts.pop().split(';').shift();
                            return null;
                        }
                        
                        const translations = {
                            workerRegion: 'WorkerÂú∞Âå∫: ',
                            detectionMethod: 'Ê£ÄÊµãÊñπÂºè: ',
                            proxyIPStatus: 'ProxyIPÁä∂ÊÄÅ: ',
                            currentIP: 'ÂΩìÂâç‰ΩøÁî®IP: ',
                            regionMatch: 'Âú∞Âå∫ÂåπÈÖç: ',
                            detectionFailed: 'Ê£ÄÊµãÂ§±Ë¥•'
                        };
                        
                        const t = translations;
                        
                        document.getElementById('regionStatus').innerHTML = t.workerRegion + '<span style="color: #ef4444;">‚ùå ' + t.detectionFailed + '</span>';
                        document.getElementById('geoInfo').innerHTML = t.detectionMethod + '<span style="color: #ef4444;">‚ùå ' + t.detectionFailed + '</span>';
                        document.getElementById('backupStatus').innerHTML = t.proxyIPStatus + '<span style="color: #ef4444;">‚ùå ' + t.detectionFailed + '</span>';
                        document.getElementById('currentIP').innerHTML = t.currentIP + '<span style="color: #ef4444;">‚ùå ' + t.detectionFailed + '</span>';
                        document.getElementById('regionMatch').innerHTML = t.regionMatch + '<span style="color: #ef4444;">‚ùå ' + t.detectionFailed + '</span>';
                        
                        // Êõ¥Êñ∞ÊâÄÊúâÁä∂ÊÄÅÁÇπ‰∏∫Á∫¢Ëâ≤
                        const regionDot = document.getElementById('regionDot');
                        const geoDot = document.getElementById('geoDot');
                        const backupDot = document.getElementById('backupDot');
                        const currentIPDot = document.getElementById('currentIPDot');
                        const regionMatchDot = document.getElementById('regionMatchDot');
                        if (regionDot) regionDot.style.background = '#ef4444';
                        if (geoDot) geoDot.style.background = '#ef4444';
                        if (backupDot) backupDot.style.background = '#ef4444';
                        if (currentIPDot) currentIPDot.style.background = '#ef4444';
                        if (regionMatchDot) regionMatchDot.style.background = '#ef4444';
                }
            }
            
                async function testAPI() {
                    try {
                        function getCookie(name) {
                            const value = '; ' + document.cookie;
                            const parts = value.split('; ' + name + '=');
                            if (parts.length === 2) return parts.pop().split(';').shift();
                            return null;
                        }
                        
                        const translations = {
                            apiTestResult: 'APIÊ£ÄÊµãÁªìÊûú: ',
                            apiTestTime: 'Ê£ÄÊµãÊó∂Èó¥: ',
                            apiTestFailed: 'APIÊ£ÄÊµãÂ§±Ë¥•: ',
                            unknownError: 'Êú™Áü•ÈîôËØØ',
                            apiTestError: 'APIÊµãËØïÂ§±Ë¥•: '
                        };
                        
                        const t = translations;
                        
                    const response = await fetch(window.location.pathname + '/test-api');
                    const data = await response.json();
                    
                    if (data.detectedRegion) {
                            alert(t.apiTestResult + data.detectedRegion + '\\n' + t.apiTestTime + data.timestamp);
                    } else {
                            alert(t.apiTestFailed + (data.error || t.unknownError));
                    }
                } catch (error) {
                        function getCookie(name) {
                            const value = '; ' + document.cookie;
                            const parts = value.split('; ' + name + '=');
                            if (parts.length === 2) return parts.pop().split(';').shift();
                            return null;
                        }
                        
                        const translations = {
                            apiTestError: 'APIÊµãËØïÂ§±Ë¥•: '
                        };
                        
                        const t = translations;
                        alert(t.apiTestError + error.message);
                }
            }
            
            // ÈÖçÁΩÆÁÆ°ÁêÜÁõ∏ÂÖ≥ÂáΩÊï∞
            async function checkKVStatus() {
                const apiUrl = window.location.pathname + '/api/config';
                
                try {
                    const response = await fetch(apiUrl);
                    
                        function getCookie(name) {
                            const value = '; ' + document.cookie;
                            const parts = value.split('; ' + name + '=');
                            if (parts.length === 2) return parts.pop().split(';').shift();
                            return null;
                        }
                        
                        const translations = {
                            kvDisabled: '‚ö†Ô∏è KVÂ≠òÂÇ®Êú™ÂêØÁî®ÊàñÊú™ÈÖçÁΩÆ',
                            kvNotConfigured: 'KVÂ≠òÂÇ®Êú™ÈÖçÁΩÆÔºåÊó†Ê≥ï‰ΩøÁî®ÈÖçÁΩÆÁÆ°ÁêÜÂäüËÉΩ„ÄÇ\\n\\nËØ∑Âú®Cloudflare Workers‰∏≠:\\n1. ÂàõÂª∫KVÂëΩÂêçÁ©∫Èó¥\\n2. ÁªëÂÆöÁéØÂ¢ÉÂèòÈáè C\\n3. ÈáçÊñ∞ÈÉ®ÁΩ≤‰ª£Á†Å',
                            kvNotEnabled: 'KVÂ≠òÂÇ®Êú™ÈÖçÁΩÆ',
                            kvEnabled: '‚úÖ KVÂ≠òÂÇ®Â∑≤ÂêØÁî®ÔºåÂèØ‰ª•‰ΩøÁî®ÈÖçÁΩÆÁÆ°ÁêÜÂäüËÉΩ',
                            kvCheckFailed: '‚ö†Ô∏è KVÂ≠òÂÇ®Ê£ÄÊµãÂ§±Ë¥•',
                            kvCheckFailedFormat: 'KVÂ≠òÂÇ®Ê£ÄÊµãÂ§±Ë¥•: ÂìçÂ∫îÊ†ºÂºèÈîôËØØ',
                            kvCheckFailedStatus: 'KVÂ≠òÂÇ®Ê£ÄÊµãÂ§±Ë¥• - Áä∂ÊÄÅÁ†Å: ',
                            kvCheckFailedError: 'KVÂ≠òÂÇ®Ê£ÄÊµãÂ§±Ë¥• - ÈîôËØØ: '
                        };
                        
                        const t = translations;
                        
                        if (response.status === 503) {
                            // KVÊú™ÈÖçÁΩÆ
                            document.getElementById('kvStatus').innerHTML = '<span style="color: #ffaa00;">' + t.kvDisabled + '</span>';
                            document.getElementById('configCard').style.display = 'block';
                            document.getElementById('currentConfig').textContent = t.kvNotConfigured;
                    } else if (response.ok) {
                        try {
                        const data = await response.json();
                        
                            // Ê£ÄÊü•ÂìçÂ∫îÊòØÂê¶ÂåÖÂê´KVÈÖçÁΩÆ‰ø°ÊÅØ
                            if (data && data.kvEnabled === true) {
                                document.getElementById('kvStatus').innerHTML = '<span style="color: #44ff44;">' + t.kvEnabled + '</span>';
                                document.getElementById('configContent').style.display = 'block';
                                document.getElementById('configCard').style.display = 'block';
                                await loadCurrentConfig();
                            } else {
                                document.getElementById('kvStatus').innerHTML = '<span style="color: #ffaa00;">' + t.kvDisabled + '</span>';
                                document.getElementById('configCard').style.display = 'block';
                                document.getElementById('currentConfig').textContent = t.kvNotEnabled;
                                }
                        } catch (jsonError) {
                            document.getElementById('kvStatus').innerHTML = '<span style="color: #ffaa00;">' + t.kvCheckFailed + '</span>';
                            document.getElementById('configCard').style.display = 'block';
                            document.getElementById('currentConfig').textContent = t.kvCheckFailedFormat;
                        }
                    } else {
                        document.getElementById('kvStatus').innerHTML = '<span style="color: #ffaa00;">' + t.kvDisabled + '</span>';
                        document.getElementById('configCard').style.display = 'block';
                        document.getElementById('currentConfig').textContent = t.kvCheckFailedStatus + response.status;
                    }
                } catch (error) {
                    function getCookie(name) {
                        const value = '; ' + document.cookie;
                        const parts = value.split('; ' + name + '=');
                        if (parts.length === 2) return parts.pop().split(';').shift();
                        return null;
                    }
                    
                    const translations = {
                        kvDisabled: '‚ö†Ô∏è KVÂ≠òÂÇ®Êú™ÂêØÁî®ÊàñÊú™ÈÖçÁΩÆ',
                        kvCheckFailedError: 'KVÂ≠òÂÇ®Ê£ÄÊµãÂ§±Ë¥• - ÈîôËØØ: '
                    };
                    
                    const t = translations;
                    
                    document.getElementById('kvStatus').innerHTML = '<span style="color: #ffaa00;">' + t.kvDisabled + '</span>';
                    document.getElementById('configCard').style.display = 'block';
                    document.getElementById('currentConfig').textContent = t.kvCheckFailedError + error.message;
                }
            }
            
            async function loadCurrentConfig() {
                const apiUrl = window.location.pathname + '/api/config';
                
                try {
                    const response = await fetch(apiUrl);
                    
                    if (response.status === 503) {
                        document.getElementById('currentConfig').textContent = 'KVÂ≠òÂÇ®Êú™ÈÖçÁΩÆÔºåÊó†Ê≥ïÂä†ËΩΩÈÖçÁΩÆ';
                        return;
                    }
                    if (!response.ok) {
                        const errorText = await response.text();
                        document.getElementById('currentConfig').textContent = 'Âä†ËΩΩÈÖçÁΩÆÂ§±Ë¥•: ' + errorText;
                        return;
                    }
                    const config = await response.json();
                    
                    // ËøáÊª§ÊéâÂÜÖÈÉ®Â≠óÊÆµ kvEnabled
                    const displayConfig = {};
                    for (const [key, value] of Object.entries(config)) {
                        if (key !== 'kvEnabled') {
                            displayConfig[key] = value;
                        }
                    }
                    
                    let configText = 'ÂΩìÂâçÈÖçÁΩÆ:\\n';
                    if (Object.keys(displayConfig).length === 0) {
                        configText += '(ÊöÇÊó†ÈÖçÁΩÆ)';
                    } else {
                        for (const [key, value] of Object.entries(displayConfig)) {
                            configText += key + ': ' + (value || '(Êú™ËÆæÁΩÆ)') + '\\n';
                        }
                    }
                    
                    document.getElementById('currentConfig').textContent = configText;
                    
                    // Êõ¥Êñ∞Ë°®ÂçïÂÄº
                    document.getElementById('wkRegion').value = config.wk || '';
                    document.getElementById('ev').checked = config.ev !== 'no';
                    document.getElementById('et').checked = config.et === 'yes';
                    document.getElementById('ex').checked = config.ex === 'yes';
                    document.getElementById('tp').value = config.tp || '';
                    document.getElementById('scu').value = config.scu || '';
                    document.getElementById('epd').checked = config.epd !== 'no';
                    document.getElementById('epi').checked = config.epi !== 'no';
                    document.getElementById('egi').checked = config.egi !== 'no';
                    if (document.getElementById('ipv4Enabled')) document.getElementById('ipv4Enabled').checked = config.ipv4 !== 'no';
                    if (document.getElementById('ipv6Enabled')) document.getElementById('ipv6Enabled').checked = config.ipv6 !== 'no';
                    if (document.getElementById('ispMobile')) document.getElementById('ispMobile').checked = config.ispMobile !== 'no';
                    if (document.getElementById('ispUnicom')) document.getElementById('ispUnicom').checked = config.ispUnicom !== 'no';
                    if (document.getElementById('ispTelecom')) document.getElementById('ispTelecom').checked = config.ispTelecom !== 'no';
                    document.getElementById('customPath').value = config.d || '';
                    document.getElementById('customIP').value = config.p || '';
                    document.getElementById('yx').value = config.yx || '';
                    document.getElementById('yxURL').value = config.yxURL || '';
                    document.getElementById('socksConfig').value = config.s || '';
                    document.getElementById('customHomepage').value = config.homepage || '';
                    document.getElementById('apiEnabled').value = config.ae || '';
                    document.getElementById('regionMatching').value = config.rm || '';
                    document.getElementById('downgradeControl').value = config.qj || '';
                    document.getElementById('portControl').value = config.dkby || '';
                    document.getElementById('preferredControl').value = config.yxby || '';
                    
                    // Êõ¥Êñ∞Ë∑ØÂæÑÁ±ªÂûãÊòæÁ§∫
                    updatePathTypeStatus(config.d);
                    
                    // Ê£ÄÊü•pÂèòÈáèÔºåÂ¶ÇÊûúÊúâÂÄºÂàôÁ¶ÅÁî®wkÂú∞Âå∫ÈÄâÊã©
                    updateWkRegionState();
                    
                } catch (error) {
                    document.getElementById('currentConfig').textContent = 'Âä†ËΩΩÈÖçÁΩÆÂ§±Ë¥•: ' + error.message;
                }
            }
            
            // Êõ¥Êñ∞Ë∑ØÂæÑÁ±ªÂûãÊòæÁ§∫
            function updatePathTypeStatus(cp) {
                const pathTypeStatus = document.getElementById('pathTypeStatus');
                const currentUrl = window.location.href;
                const pathParts = window.location.pathname.split('/').filter(p => p);
                const currentPath = pathParts.length > 0 ? pathParts[0] : '';
                
                if (cp && cp.trim()) {
                    // ‰ΩøÁî®Ëá™ÂÆö‰πâË∑ØÂæÑ (d)
                    pathTypeStatus.innerHTML = '<div style="color: #44ff44;">‰ΩøÁî®Á±ªÂûã: <strong>Ëá™ÂÆö‰πâË∑ØÂæÑ (d)</strong></div>' +
                        '<div style="margin-top: 5px; color: #00ff00;">ÂΩìÂâçË∑ØÂæÑ: <span style="color: #ffaa00;">' + cp + '</span></div>' +
                        '<div style="margin-top: 5px; font-size: 0.9rem; color: #00aa00;">ËÆøÈóÆÂú∞ÂùÄ: ' + 
                        (currentUrl.split('/')[0] + '//' + currentUrl.split('/')[2]) + cp + '/sub</div>';
                } else {
                    // ‰ΩøÁî® UUID (u)
                    pathTypeStatus.innerHTML = '<div style="color: #44ff44;">‰ΩøÁî®Á±ªÂûã: <strong>UUID Ë∑ØÂæÑ (u)</strong></div>' +
                        '<div style="margin-top: 5px; color: #00ff00;">ÂΩìÂâçË∑ØÂæÑ: <span style="color: #ffaa00;">' + (currentPath || '(UUID)') + '</span></div>' +
                        '<div style="margin-top: 5px; font-size: 0.9rem; color: #00aa00;">ËÆøÈóÆÂú∞ÂùÄ: ' + currentUrl.split('/sub')[0] + '/sub</div>';
                }
            }
            
            // Êõ¥Êñ∞wkÂú∞Âå∫ÈÄâÊã©ÁöÑÂêØÁî®/Á¶ÅÁî®Áä∂ÊÄÅ
            async function updateWkRegionState() {
                const customIPInput = document.getElementById('customIP');
                const wkRegion = document.getElementById('wkRegion');
                const wkRegionHint = document.getElementById('wkRegionHint');
                
                if (customIPInput && wkRegion) {
                    const hasCustomIP = customIPInput.value.trim() !== '';
                    wkRegion.disabled = hasCustomIP;
                    
                    // Ê∑ªÂä†ËßÜËßâÂèçÈ¶à
                    if (hasCustomIP) {
                        wkRegion.style.opacity = '0.5';
                        wkRegion.style.cursor = 'not-allowed';
                        wkRegion.style.backgroundColor = 'rgba(0, 0, 0, 0.5)';
                        // ÊòæÁ§∫ÊèêÁ§∫‰ø°ÊÅØ
                        if (wkRegionHint) {
                            wkRegionHint.style.display = 'block';
                            wkRegionHint.style.color = '#ffaa00';
                        }
                    } else {
                        wkRegion.style.opacity = '1';
                        wkRegion.style.cursor = 'pointer';
                        wkRegion.style.backgroundColor = 'rgba(0, 0, 0, 0.8)';
                        // ÈöêËóèÊèêÁ§∫‰ø°ÊÅØ
                        if (wkRegionHint) {
                            wkRegionHint.style.display = 'none';
                        }
                    }
                }
                
                // Ê£ÄÊü•ÈîôËØØÊó•ÂøóÊï∞Èáè
                await checkErrorLogsCount();
            }
            
            // Ê£ÄÊü•ÈîôËØØÊó•ÂøóÊï∞Èáè
            async function checkErrorLogsCount() {
                try {
                    const response = await fetch('/api/error-logs?limit=1');
                    const errorLogsDot = document.getElementById('errorLogsDot');
                    if (response.ok) {
                        const data = await response.json();
                        const totalCount = data.total || 0;
                        const errorCount = data.logs.filter(log => log.type !== 'String').length;
                        const errorLogsCountEl = document.getElementById('errorLogsCount');
                        if (errorLogsCountEl) {
                            if (totalCount === 0) {
                                errorLogsCountEl.textContent = '0 Êù°';
                                errorLogsCountEl.style.color = '#666';
                                if (errorLogsDot) errorLogsDot.style.background = '#666';
                            } else {
                                errorLogsCountEl.textContent = totalCount + ' Êù°';
                                if (errorCount > 0) {
                                    errorLogsCountEl.style.color = '#ff6b6b';
                                    if (errorLogsDot) errorLogsDot.style.background = '#ff6b6b';
                                } else {
                                    errorLogsCountEl.style.color = '#22c55e';
                                    if (errorLogsDot) errorLogsDot.style.background = '#22c55e';
                                }
                            }
                        }
                    } else {
                        const errorLogsCountEl = document.getElementById('errorLogsCount');
                        if (errorLogsCountEl) {
                            errorLogsCountEl.textContent = 'Âä†ËΩΩÂ§±Ë¥•';
                            errorLogsCountEl.style.color = '#ff6b6b';
                        }
                        if (errorLogsDot) errorLogsDot.style.background = '#ff6b6b';
                    }
                } catch (error) {
                    const errorLogsCountEl = document.getElementById('errorLogsCount');
                    const errorLogsDot = document.getElementById('errorLogsDot');
                    if (errorLogsCountEl) {
                        errorLogsCountEl.textContent = 'Âä†ËΩΩÂ§±Ë¥•';
                        errorLogsCountEl.style.color = '#ff6b6b';
                    }
                    if (errorLogsDot) errorLogsDot.style.background = '#ff6b6b';
                }
            }
            
            // ‰∏ãËΩΩÈîôËØØÊó•Âøó
            async function downloadErrorLogs() {
                try {
                    const btn = document.getElementById('downloadErrorLogsBtn');
                    if (btn) {
                        btn.disabled = true;
                        btn.textContent = '‰∏ãËΩΩ‰∏≠...';
                    }
                    
                    const response = await fetch('/api/error-logs?limit=1000');
                    if (!response.ok) {
                        throw new Error('Ëé∑ÂèñÊó•ÂøóÂ§±Ë¥•: ' + response.status);
                    }
                    
                    const data = await response.json();
                    const logs = data.logs || [];
                    
                    if (logs.length === 0) {
                        alert('ÂΩìÂâçÊ≤°ÊúâÈîôËØØÊó•Âøó');
                        if (btn) {
                            btn.disabled = false;
                            btn.textContent = 'üì• ‰∏ãËΩΩÊó•Âøó';
                        }
                        return;
                    }
                    
                    // Ê†ºÂºèÂåñÊó•Âøó‰∏∫ÊñáÊú¨
                    const logText = logs.map(log => {
                        let text = '[' + log.timestamp + '] [' + log.context + '] ' + log.message;
                        if (log.stack) {
                            text += '\\nÂ†ÜÊ†à:\\n' + log.stack;
                        }
                        if (log.url) {
                            text += '\\nURL: ' + log.url;
                        }
                        if (log.method) {
                            text += '\\nÊñπÊ≥ï: ' + log.method;
                        }
                        if (log.workerRegion) {
                            text += '\\nÂú∞Âå∫: ' + log.workerRegion;
                        }
                        return text;
                    }).join('\\n\\n' + '='.repeat(80) + '\\n\\n');
                    
                    // ÂàõÂª∫‰∏ãËΩΩ
                    const dataBlob = new Blob([logText], { type: 'text/plain; charset=utf-8' });
                    const url = URL.createObjectURL(dataBlob);
                    const link = document.createElement('a');
                    link.href = url;
                    link.download = 'error-logs-' + new Date().toISOString().split('T')[0] + '-' + Date.now() + '.txt';
                    document.body.appendChild(link);
                    link.click();
                    document.body.removeChild(link);
                    URL.revokeObjectURL(url);
                    
                    // ÂêåÊó∂‰∏ãËΩΩJSONÊ†ºÂºè
                    const jsonBlob = new Blob([JSON.stringify(logs, null, 2)], { type: 'application/json; charset=utf-8' });
                    const jsonUrl = URL.createObjectURL(jsonBlob);
                    const jsonLink = document.createElement('a');
                    jsonLink.href = jsonUrl;
                    jsonLink.download = 'error-logs-' + new Date().toISOString().split('T')[0] + '-' + Date.now() + '.json';
                    document.body.appendChild(jsonLink);
                    setTimeout(() => {
                        jsonLink.click();
                        document.body.removeChild(jsonLink);
                        URL.revokeObjectURL(jsonUrl);
                    }, 300);
                    
                    if (btn) {
                        btn.disabled = false;
                        btn.textContent = 'üì• ‰∏ãËΩΩÊó•Âøó';
                    }
                    
                    alert('ÈîôËØØÊó•ÂøóÂ∑≤‰∏ãËΩΩÔºàTXTÂíåJSONÊ†ºÂºèÔºâ');
                } catch (error) {
                    alert('‰∏ãËΩΩÂ§±Ë¥•: ' + error.message);
                    const btn = document.getElementById('downloadErrorLogsBtn');
                    if (btn) {
                        btn.disabled = false;
                        btn.textContent = 'üì• ‰∏ãËΩΩÊó•Âøó';
                    }
                }
            }
            
            async function saveConfig(configData) {
                const apiUrl = window.location.pathname + '/api/config';
                
                try {
                    const response = await fetch(apiUrl, {
                        method: 'POST',
                        headers: { 'Content-Type': 'application/json' },
                        body: JSON.stringify(configData)
                    });
                    
                    
                    if (response.status === 503) {
                        showStatus('KVÂ≠òÂÇ®Êú™ÈÖçÁΩÆÔºåÊó†Ê≥ï‰øùÂ≠òÈÖçÁΩÆ„ÄÇËØ∑ÂÖàÂú®Cloudflare Workers‰∏≠ÈÖçÁΩÆKVÂ≠òÂÇ®„ÄÇ', 'error');
                        return;
                    }
                    
                    if (!response.ok) {
                        const errorText = await response.text();
                        
                        // Â∞ùËØïËß£Êûê JSON ÈîôËØØ‰ø°ÊÅØ
                        try {
                            const errorData = JSON.parse(errorText);
                            showStatus(errorData.message || '‰øùÂ≠òÂ§±Ë¥•', 'error');
                        } catch (parseError) {
                            // Â¶ÇÊûú‰∏çÊòØ JSONÔºåÁõ¥Êé•ÊòæÁ§∫ÊñáÊú¨
                            showStatus('‰øùÂ≠òÂ§±Ë¥•: ' + errorText, 'error');
                        }
                        return;
                    }
                    
                    const result = await response.json();
                    
                    showStatus(result.message, result.success ? 'success' : 'error');
                    
                    if (result.success) {
                        await loadCurrentConfig();
                        // Êõ¥Êñ∞wkÂú∞Âå∫ÈÄâÊã©Áä∂ÊÄÅ
                        updateWkRegionState();
                        // ‰øùÂ≠òÊàêÂäüÂêéÂà∑Êñ∞È°µÈù¢‰ª•Êõ¥Êñ∞Á≥ªÁªüÁä∂ÊÄÅ
                        setTimeout(function() {
                            window.location.reload();
                        }, 1500);
                    }
                } catch (error) {
                    showStatus('‰øùÂ≠òÂ§±Ë¥•: ' + error.message, 'error');
                }
            }
            
            function showStatus(message, type) {
                const statusDiv = document.getElementById('statusMessage');
                statusDiv.textContent = message;
                statusDiv.style.display = 'block';
                statusDiv.style.color = type === 'success' ? '#00ff00' : '#ff0000';
                statusDiv.style.borderColor = type === 'success' ? '#00ff00' : '#ff0000';
                
                setTimeout(function() {
                    statusDiv.style.display = 'none';
                }, 3000);
            }
            
            async function resetAllConfig() {
                if (confirm('Á°ÆÂÆöË¶ÅÈáçÁΩÆÊâÄÊúâÈÖçÁΩÆÂêóÔºüËøôÂ∞ÜÊ∏ÖÁ©∫ÊâÄÊúâKVÈÖçÁΩÆÔºåÊÅ¢Â§ç‰∏∫ÁéØÂ¢ÉÂèòÈáèËÆæÁΩÆ„ÄÇ')) {
                    try {
                        const response = await fetch(window.location.pathname + '/api/config', {
                            method: 'POST',
                            headers: { 'Content-Type': 'application/json' },
                            body: JSON.stringify({ 
                                wk: '',
                                d: '',
                                p: '',
                                yx: '',
                                yxURL: '',
                                s: '', ae: '',
                                rm: '',
                                qj: '',
                                dkby: '',
                                yxby: '', ev: '', et: '', ex: '', tp: '', scu: '', epd: '', epi: '', egi: '',
                                ipv4: '', ipv6: '', ispMobile: '', ispUnicom: '', ispTelecom: '',
                                homepage: ''
                            })
                        });
                        
                        if (response.status === 503) {
                            showStatus('KVÂ≠òÂÇ®Êú™ÈÖçÁΩÆÔºåÊó†Ê≥ïÈáçÁΩÆÈÖçÁΩÆ„ÄÇ', 'error');
                            return;
                        }
                        
                        if (!response.ok) {
                            const errorText = await response.text();
                            
                            // Â∞ùËØïËß£Êûê JSON ÈîôËØØ‰ø°ÊÅØ
                            try {
                                const errorData = JSON.parse(errorText);
                                showStatus(errorData.message || 'ÈáçÁΩÆÂ§±Ë¥•', 'error');
                            } catch (parseError) {
                                // Â¶ÇÊûú‰∏çÊòØ JSONÔºåÁõ¥Êé•ÊòæÁ§∫ÊñáÊú¨
                                showStatus('ÈáçÁΩÆÂ§±Ë¥•: ' + errorText, 'error');
                            }
                            return;
                        }
                        
                        const result = await response.json();
                        showStatus(result.message || 'ÈÖçÁΩÆÂ∑≤ÈáçÁΩÆ', result.success ? 'success' : 'error');
                        
                        if (result.success) {
                            await loadCurrentConfig();
                            // Êõ¥Êñ∞wkÂú∞Âå∫ÈÄâÊã©Áä∂ÊÄÅ
                            updateWkRegionState();
                            // Âà∑Êñ∞È°µÈù¢‰ª•Êõ¥Êñ∞Á≥ªÁªüÁä∂ÊÄÅ
                            setTimeout(function() {
                                window.location.reload();
                            }, 1500);
                        }
                    } catch (error) {
                        showStatus('ÈáçÁΩÆÂ§±Ë¥•: ' + error.message, 'error');
                    }
                }
            }
            
            document.addEventListener('DOMContentLoaded', function() {
                checkSystemStatus();
                checkKVStatus();
                checkErrorLogsCount();
                
                // ÁõëÂê¨customIPËæìÂÖ•Ê°ÜÂèòÂåñÔºåÂÆûÊó∂Êõ¥Êñ∞wkÂú∞Âå∫ÈÄâÊã©Áä∂ÊÄÅ
                const customIPInput = document.getElementById('customIP');
                if (customIPInput) {
                    customIPInput.addEventListener('input', function() {
                        updateWkRegionState();
                    });
                }
                
                // ÁªëÂÆöË°®Âçï‰∫ã‰ª∂
                const regionForm = document.getElementById('regionForm');
                if (regionForm) {
                    regionForm.addEventListener('submit', async function(e) {
                        e.preventDefault();
                        const wkRegion = document.getElementById('wkRegion').value;
                        await saveConfig({ wk: wkRegion });
                    });
                }
                
                const saveProtocolBtn = document.getElementById('saveProtocolBtn');
                if (saveProtocolBtn) {
                    saveProtocolBtn.addEventListener('click', async function(e) {
                        e.preventDefault();
                        const configData = { 
                            ev: document.getElementById('ev').checked ? 'yes' : 'no', 
                            et: document.getElementById('et').checked ? 'yes' : 'no', 
                            ex: document.getElementById('ex').checked ? 'yes' : 'no', 
                            tp: document.getElementById('tp').value
                        };
                        
                        if (!document.getElementById('ev').checked && 
                            !document.getElementById('et').checked && 
                            !document.getElementById('ex').checked) {
                            alert('Ëá≥Â∞ëÈúÄË¶ÅÂêØÁî®‰∏Ä‰∏™ÂçèËÆÆÔºÅ');
                            return;
                        }
                        
                        await saveConfig(configData);
                    });
                }
                
                const otherConfigForm = document.getElementById('otherConfigForm');
                if (otherConfigForm) {
                    otherConfigForm.addEventListener('submit', async function(e) {
                        e.preventDefault();
                        const configData = { ev: document.getElementById('ev').checked ? 'yes' : 'no', et: document.getElementById('et').checked ? 'yes' : 'no', ex: document.getElementById('ex').checked ? 'yes' : 'no', tp: document.getElementById('tp').value,
                            d: document.getElementById('customPath').value,
                            p: document.getElementById('customIP').value,
                            yx: document.getElementById('yx').value,
                            yxURL: document.getElementById('yxURL').value,
                            s: document.getElementById('socksConfig').value,
                            homepage: document.getElementById('customHomepage').value
                        };
                        
                        // Á°Æ‰øùËá≥Â∞ëÈÄâÊã©‰∏Ä‰∏™ÂçèËÆÆ
                        if (!document.getElementById('ev').checked && 
                            !document.getElementById('et').checked && 
                            !document.getElementById('ex').checked) {
                            alert('Ëá≥Â∞ëÈúÄË¶ÅÂêØÁî®‰∏Ä‰∏™ÂçèËÆÆÔºÅ');
                            return;
                        }
                        
                        await saveConfig(configData);
                    });
                }
                
                const advancedConfigForm = document.getElementById('advancedConfigForm');
                if (advancedConfigForm) {
                    advancedConfigForm.addEventListener('submit', async function(e) {
                        e.preventDefault();
                        const configData = { scu: document.getElementById('scu').value, epd: document.getElementById('epd').checked ? 'yes' : 'no', epi: document.getElementById('epi').checked ? 'yes' : 'no', egi: document.getElementById('egi').checked ? 'yes' : 'no', ae: document.getElementById('apiEnabled').value,
                            rm: document.getElementById('regionMatching').value,
                            qj: document.getElementById('downgradeControl').value,
                            dkby: document.getElementById('portControl').value,
                            yxby: document.getElementById('preferredControl').value,
                            ipv4: document.getElementById('ipv4Enabled').checked ? 'yes' : 'no',
                            ipv6: document.getElementById('ipv6Enabled').checked ? 'yes' : 'no',
                            ispMobile: document.getElementById('ispMobile').checked ? 'yes' : 'no',
                            ispUnicom: document.getElementById('ispUnicom').checked ? 'yes' : 'no',
                            ispTelecom: document.getElementById('ispTelecom').checked ? 'yes' : 'no'
                        };
                        await saveConfig(configData);
                    });
                }
                
                let testAbortController = null;
                let testResults = [];
                
                const startTestBtn = document.getElementById('startLatencyTest');
                const stopTestBtn = document.getElementById('stopLatencyTest');
                const testStatus = document.getElementById('latencyTestStatus');
