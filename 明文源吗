                radial-gradient(circle at 0% 0%, rgba(255, 255, 255, 0.98) 0%, transparent 40%),
                radial-gradient(circle at 100% 100%, rgba(34, 197, 94, 0.05) 0%, transparent 50%),
                linear-gradient(135deg, #ffffff 0%, #f9fafb 25%, #ffffff 50%, #f0fdf4 75%, #ffffff 100%);
            background-size: 100% 100%;
            background-attachment: fixed;
            background-repeat: no-repeat;
            background-position: center center;
            min-height: 100vh;
